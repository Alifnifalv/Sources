            <Value>=Parameters!SchoolID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 0 AS EmployeeIID,'ALL' AS EmployeeName where len(isnull(@EmployeeSearch,''))=0 
UNION ALL
SELECT EmployeeIID, ISNULL(EmployeeCode,'')+' - '+ISNULL(FirstName,'')+' '+ISNULL(MiddleName,'')+' '+ISNULL(LastName,'') as EmployeeName
FROM payroll.Employees 
WHERE len(isnull(@EmployeeSearch,''))=0 and DesignationID = IIF(@Designation=0,DesignationID,@Designation) and BranchID=@SchoolID
UNION ALL
SELECT EmployeeIID, ISNULL(EmployeeCode,'')+' - '+ISNULL(FirstName,'')+' '+ISNULL(MiddleName,'')+' '+ISNULL(LastName,'') as EmployeeName
FROM payroll.Employees
WHERE EmployeeCode + ' - ' + RTRIM(LTRIM(ISNULL(FirstName,'') +' ' + ISNULL(MiddleName,'') +' ' + ISNULL(LastName,''))) LIKE '%' +isnull(@EmployeeSearch,'') +'%' AND len(isnull(@EmployeeSearch,''))&gt;0
and DesignationID = IIF(@Designation=0,DesignationID,@Designation) and BranchID=@SchoolID</CommandText>
