        </QueryParameters>
        <CommandText>Select top(6) a.*,c.ContentData as PrincipleSign
from schools.EmployeeIDCardReportView as a
  left join payroll.employees as e on e.BranchID = a.BranchID
			and e.IsActive=1 and e.DesignationID = (Select SettingValue from setting.Settings where settingcode = 'EMP_PRINCIPAL_DESIGNATION_ID')
  left join contents.ContentFiles as c on c.ContentFileIID = e.SignatureContentID
Where a.EmployeeIID=IIF(@Employee_IDs='0',a.EmployeeIID ,@Employee_IDs) and a.DesignationID=IIF(@Designation=0,a.DesignationID ,@Designation) and a.BranchID = @SchoolID
order by a.EmployeeCode desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
