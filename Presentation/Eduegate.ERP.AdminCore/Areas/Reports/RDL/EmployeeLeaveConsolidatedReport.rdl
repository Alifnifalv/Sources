    <DataSet Name="Employee">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 0 AS EmployeeIID, 'All' AS EmployeeName 

UNION ALL

SELECT emp.EmployeeIID, emp.EmployeeCode + ' - ' + emp.FirstName + ' ' + ISNULL(emp.MiddleName, '') + ' ' + emp.LastName AS EmployeeName
FROM payroll.Employees emp</CommandText>
      </Query>
