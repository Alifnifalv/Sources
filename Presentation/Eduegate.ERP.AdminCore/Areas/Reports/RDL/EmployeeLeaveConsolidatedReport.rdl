    <DataSet Name="LeaveTypes">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 0 AS LeaveTypeID, 'All' AS Description 
UNION ALL
SELECT LeaveTypeID,Description FROM payroll.LeaveTypes
UNION ALL 
SELECT 20 AS LeaveTypeID, 'LOP' AS Description </CommandText>
      </Query>
      <Fields>
