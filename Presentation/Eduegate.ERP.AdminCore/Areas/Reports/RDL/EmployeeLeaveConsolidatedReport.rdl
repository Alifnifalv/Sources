  <DataSets>
    <DataSet Name="StudentAttendance">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ENDDATE">
            <Value>=Parameters!ENDDATE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@STARTDATE">
            <Value>=Parameters!STARTDATE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EmployeeID">
            <Value>=Parameters!EmployeeID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LeaveTypeIDs">
            <Value>=Parameters!LeaveTypeIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DepartmentID">
            <Value>=Parameters!DepartmentID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IsActive">
            <Value>=Parameters!IsActive.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>;WITH Leaves AS (
    SELECT 
        la.LeaveApplicationIID, 
        la.EmployeeID, 
        la.LeaveTypeID, 
        lt.Description, 
        la.FromDate, 
        la.ToDate, 
        CASE 
            WHEN la.IsHalfDay = 1 THEN 0.5
            ELSE DATEDIFF(DAY, 
                CASE WHEN la.FromDate &lt; @STARTDATE THEN @STARTDATE ELSE la.FromDate END,
                CASE WHEN la.ToDate &gt; @ENDDATE THEN @ENDDATE ELSE la.ToDate END
            ) + 1
        END AS LeaveDays,
        la.IsHalfDay
    FROM payroll.LeaveApplications la
    LEFT JOIN payroll.LeaveTypes lt ON lt.LeaveTypeID = la.LeaveTypeID
    WHERE la.LeaveStatusID = 2
)
SELECT 
    SUM(lv.LeaveDays) AS TotalLeaveDays,
    emp.EmployeeIID, 
    emp.EmployeeCode, 
    TRIM(emp.FirstName + ' ' + ISNULL(emp.MiddleName, '') + ' ' + emp.LastName) AS EmployeeName,
    lv.LeaveTypeID,
    lv.Description,
    IIF(@EmployeeID = 0, 'All', TRIM(emp.FirstName + ' ' + ISNULL(emp.MiddleName, '') + ' ' + emp.LastName)) AS SelectedEmployee,
    IIF(@DepartmentID = 0, 'All', dp.DepartmentName) AS SelectedDepartment,
    IIF(@IsActive = 2, 'Both', IIF(emp.IsActive = 1, 'Active', 'Inactive')) AS IsActive
FROM payroll.Employees emp
LEFT JOIN Leaves lv ON lv.EmployeeID = emp.EmployeeIID
LEFT JOIN mutual.Departments dp ON dp.DepartmentID = emp.DepartmentID
WHERE 
    (lv.FromDate &lt;= CAST(@ENDDATE AS DATETIME)) 
    AND (lv.ToDate &gt;= CAST(@STARTDATE AS DATETIME)) 
    AND emp.EmployeeIID = IIF(@EmployeeID = 0, emp.EmployeeIID, @EmployeeID)
    AND (@LeaveTypeIDs = '0' OR lv.LeaveTypeID IN (SELECT value FROM STRING_SPLIT(@LeaveTypeIDs, ',')))
    AND emp.DepartmentID = IIF(@DepartmentID = 0, emp.DepartmentID, @DepartmentID)
    AND ISNULL(emp.IsActive, 0) = IIF(@IsActive = 2, emp.IsActive, @IsActive)
GROUP BY 
    emp.EmployeeIID, 
    emp.EmployeeCode, 
    emp.FirstName, 
    emp.MiddleName, 
    emp.LastName, 
    lv.LeaveTypeID, 
    lv.Description,
    dp.DepartmentName,
    emp.IsActive</CommandText>
      </Query>
