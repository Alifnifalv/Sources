  <DataSets>
    <DataSet Name="Concession">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ACADEMICYEARID">
            <Value>=Parameters!ACADEMICYEARID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>;WITH STAFFCONCESSION AS
(
	SELECT cs.StudentFeeConcessionIID,std.StudentIID,std.AdmissionNumber + ' - ' + RTRIM(LTRIM(ISNULL(std.FirstName,'') +' ' + ISNULL(std.MiddleName,'') +' ' + ISNULL(std.LastName,''))) AS StudentName,
	emp.EmployeeIID AS ConcessionPersonID, emp.EmployeeCode + ' - ' + emp.FirstName + ' ' + ISNULL(emp.MiddleName + ' ','') + ISNULL(emp.LastName,'') AS ConcessionPerson,
	fm.FeeMasterID, fm.Description AS FeeMaster, cs.FeePeriodID, fp.Description AS FeePeriod, cs.AcademicYearID, acy.Description AS AcademicYear,
	cs.PercentageAmount, cs.DueAmount AS TotalAmount, cs.ConcessionAmount
	--* 
	FROM schools.StudentFeeConcessions cs
	LEFT JOIN schools.Students std ON std.StudentIID = cs.StudentID
	INNER JOIN payroll.Employees emp ON emp.EmployeeIID = cs.StaffID
	LEFT JOIN schools.AcademicYears acy ON acy.AcademicYearID = cs.AcademicYearID
	LEFT JOIN schools.FeeMasters fm ON fm.FeeMasterID = cs.FeeMasterID
	LEFT JOIN schools.FeePeriods fp ON fp.FeePeriodID = cs.FeePeriodID
	WHERE cs.AcademicYearID = @ACADEMICYEARID
),
PARENTCONCESSION AS
(
	SELECT cs.StudentFeeConcessionIID,std.StudentIID,std.AdmissionNumber + ' - ' + RTRIM(LTRIM(ISNULL(std.FirstName,'') +' ' + ISNULL(std.MiddleName,'') +' ' + ISNULL(std.LastName,''))) AS StudentName,
	pnt.ParentIID AS ConcessionPersonID, pnt.ParentCode +'-'+ case when pnt.FatherFirstName IS NULL  then pnt.FatherName  else ISNULL(pnt.FatherFirstName,'')+' '+ISNULL(pnt.FatherMiddleName,'')+' '+ISNULL(pnt.FatherLastName,'') end AS ConcessionPerson,
	fm.FeeMasterID, fm.Description AS FeeMaster, cs.FeePeriodID, fp.Description AS FeePeriod, cs.AcademicYearID, acy.Description AS AcademicYear,
	cs.PercentageAmount, cs.DueAmount AS TotalAmount, cs.ConcessionAmount
	--* 
	FROM schools.StudentFeeConcessions cs
	LEFT JOIN schools.Students std ON std.StudentIID = cs.StudentID
	INNER JOIN schools.Parents pnt ON pnt.ParentIID = cs.ParentID
	LEFT JOIN schools.AcademicYears acy ON acy.AcademicYearID = cs.AcademicYearID
	LEFT JOIN schools.FeeMasters fm ON fm.FeeMasterID = cs.FeeMasterID
	LEFT JOIN schools.FeePeriods fp ON fp.FeePeriodID = cs.FeePeriodID
	WHERE cs.AcademicYearID = @ACADEMICYEARID
),
ALLCONCESSION AS 
(
	SELECT * FROM STAFFCONCESSION
	UNION ALL
	SELECT * FROM PARENTCONCESSION
)
SELECT * FROM ALLCONCESSION
ORDER BY StudentFeeConcessionIID</CommandText>
      </Query>
