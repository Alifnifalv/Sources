<div class="@(Model ? "form-group" : "sku-fielditems")">
    <label class="addprodleft control-label">Allow Shipping for:</label>
    <div class="@(Model ? "controls" : string.Empty)">
        @if (Model)
        {
            <ui-select multiple style="width: 100%;" ng-model="ProductModel.QuickCreate.ProductInventoryConfigViewModels.AllowShippingfor" theme="select2" class="pos-txt">
                <ui-select-match placeholder="Search Country">{{$item.CountryName}}</ui-select-match>
                <ui-select-choices repeat="countries in ProductModel.QuickCreate.Countries|filter:$select.search">{{countries.CountryName}}</ui-select-choices>
            </ui-select>
        }
        else
        {
            <ui-select multiple style="width: 100%;" ng-model="sku.ProductInventoryConfigViewModels.AllowShippingfor" theme="select2" class="pos-txt">
                <ui-select-match placeholder="Search Country">{{$item.CountryName}}</ui-select-match>
