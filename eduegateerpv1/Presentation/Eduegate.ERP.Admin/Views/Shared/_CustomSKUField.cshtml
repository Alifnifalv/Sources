    </div>

</div>

<div class="@(Model ? "form-group" : "sku-fielditems")">
    <label class="addprodleft control-label">Tags</label>
    <div class="@(Model ? "controls" : string.Empty)">

        <ui-select multiple tagging="tagTransformV2" style="width: 100%;" on-select="UniqueTagV2($item,sku.ProductInventoryConfigViewModels.Tags)" ng-model="@(Model ? "ProductModel.QuickCreate.ProductInventoryConfigViewModels.Tags" : "sku.ProductInventoryConfigViewModels.Tags")"
                   theme="select2" class="pos-txt" ng-disabled="disabled">
            <ui-select-match placeholder="Select New/Exist tag...">{{$item.Value}}</ui-select-match>
            <ui-select-choices repeat="tag in SKUTags | propsFilter: {Value: $select.search}">
                <div ng-bind-html="tag.Value | highlight: $select.search"></div>
            </ui-select-choices>
        </ui-select>
    </div>

</div>

<div class="@(Model ? "form-group" : "sku-fielditems")">
    <label class="addprodleft control-label">SKU Owner</label>
    <div class="controls brand">
        <ui-select ng-model="@(Model ? "ProductModel.QuickCreate.ProductInventoryConfigViewModels.SKUOwner" : "sku.ProductInventoryConfigViewModels.SKUOwner")" theme="select2" class="pos-txt">
            <ui-select-match placeholder="Select owner">{{$select.selected.Value}}</ui-select-match>
            <ui-select-choices repeat="employee in employees|filter:$select.search" refresh="GetEmployeeList($select.search)"
                               refresh-delay="50">
                {{employee.Value}}
            </ui-select-choices>
        </ui-select>
    </div>
</div>

@if (!Model)
{
    <div class="sku-fielditems" ng-repeat="property in sku.Properties">
        <label class="addprodleft control-label">{{property.PropertyName}}</label>
        <div>
            <input type="text" ng-model="property.DefaultValue">
        </div>
    </div>
}
else
{
    <div class="form-group" ng-repeat="defaultProperty in ProductModel.QuickCreate.DefaultProperties">
        <label class="addprodleft control-label">{{defaultProperty.PropertyName}}</label>
        <div class="controls productname">
            <input type="text" ng-model="defaultProperty.DefaultValue">
        </div>
    </div>
}
