@model Eduegate.Web.Library.ViewModels.SearchListViewModel
@{
    Layout = null;
    var viewIID = Eduegate.Services.Contracts.Enums.SearchView.PurchaseReturn;
}

<div id="PurchaseReturnLists" class="windowcontainer">
    <div ng-controller="ViewController" ng-init="init(@Newtonsoft.Json.JsonConvert.SerializeObject(Model), '@viewIID');InitializeWayPoint();">
        <div class="pageheadername"><div>View Purchase Returns</div><div class="alignright menugroup"><div class="createprodlink button-orange alignright" ng-click="Create('@viewIID')"><a title="Create Purchase Returns"><i class="fa fa-plus"></i><span>Create Purchase Returns</span></a></div></div></div>
                @await Html.PartialAsync("_SummaryHeader", Model)
        @await Html.PartialAsync("_List", Model)
    </div>
</div>