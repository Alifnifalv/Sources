  <DataSets>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SubjectTypeID">
            <Value>=Parameters!SubjectTypeID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AcademicYearID">
            <Value>=Parameters!AcademicYearID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>;WITH STUD AS
(
	SELECT CAST(Count(stud.StudentIID) AS FLOAT) as StudentCount,cls.ClassDescription,stud.ClassID,cls.ORDERNO
	,stud.AcademicYearID,acy.Description AS AcademicYear
	FROM schools.Students stud
	INNER JOIN schools.Classes cls ON cls.ClassID = stud.ClassID
	INNER JOIN schools.AcademicYears acy ON acy.AcademicYearID = stud.AcademicYearID
	WHERE stud.IsActive = 1 AND stud.Status = 1
	GROUP BY stud.ClassID,stud.AcademicYearID,cls.ClassDescription,cls.ORDERNO,acy.Description
),
CLSTCHR AS
(
	SELECT CAST(COUNT(DISTINCT ctm.TeacherID) AS FLOAT) AS TeacherCount,ctm.ClassID,ctm.AcademicYearID
	FROM schools.ClassTeacherMaps ctm
	INNER JOIN schools.Subjects sub ON sub.SubjectID = ctm.SubjectID
	LEFT JOIN schools.SubjectTypes sbt  ON sbt.SubjectTypeID = sub.SubjectTypeID AND @SubjectTypeID = 1 WHERE (@SubjectTypeID = 0 OR sbt.SubjectTypeID = 1)
	GROUP BY ctm.ClassID,ctm.AcademicYearID,sbt.SubjectTypeID
)
SELECT CAST(StudentCount / dbo.GCD(StudentCount, TeacherCount) AS VARCHAR(10)) + ':' + CAST(TeacherCount / dbo.GCD(StudentCount, TeacherCount) AS VARCHAR(10)) AS Ratio
,st.StudentCount,ct.TeacherCount,st.ClassID,st.ClassDescription,st.AcademicYearID,st.AcademicYear,IIF(@SubjectTypeID = 0,'All Subjects','Core Subjects') AS Type
FROM STUD st
INNER JOIN CLSTCHR ct ON ct.ClassID = st.ClassID AND ct.AcademicYearID = st.AcademicYearID
WHERE st.AcademicYearID = @AcademicYearID
ORDER BY st.ORDERNO
</CommandText>
      </Query>
