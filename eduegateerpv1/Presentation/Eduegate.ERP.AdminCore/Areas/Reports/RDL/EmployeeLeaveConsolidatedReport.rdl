  <DataSets>
    <DataSet Name="StudentAttendance">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FRSTDATE">
            <Value>=Parameters!FRSTDATE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FRENDATE">
            <Value>=Parameters!FRENDATE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TOSTDATE">
            <Value>=Parameters!TOSTDATE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TOENDATE">
            <Value>=Parameters!TOENDATE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EmployeeID">
            <Value>=Parameters!EmployeeID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>;WITH CASUAL AS
(
SELECT la.EmployeeID,la.LeaveTypeID,lt.Description,la.FromDate--,* 
FROM payroll.LeaveApplications la
LEFT JOIN payroll.LeaveTypes lt ON lt.LeaveTypeID = la.LeaveTypeID
WHERE la.LeaveTypeID = 2 
AND CAST(la.FromDate AS DATETIME) BETWEEN CAST(@FRSTDATE AS DATETIME) AND CAST(@FRENDATE AS DATETIME) 
AND CAST(la.ToDate AS DATETIME) BETWEEN CAST(@TOSTDATE AS DATETIME) AND CAST(@TOENDATE AS DATETIME) 
),
SICK AS
(
SELECT la.EmployeeID,la.LeaveTypeID,lt.Description,la.FromDate--,* 
FROM payroll.LeaveApplications la
LEFT JOIN payroll.LeaveTypes lt ON lt.LeaveTypeID = la.LeaveTypeID
WHERE la.LeaveTypeID = 3 AND CAST(la.FromDate AS DATETIME) BETWEEN CAST(@FRSTDATE AS DATETIME) AND CAST(@FRENDATE AS DATETIME) 
AND CAST(la.ToDate AS DATETIME) BETWEEN CAST(@TOSTDATE AS DATETIME) AND CAST(@TOENDATE AS DATETIME)
),
SHORT AS
(
SELECT la.EmployeeID,la.LeaveTypeID,lt.Description,la.FromDate--,* 
FROM payroll.LeaveApplications la
LEFT JOIN payroll.LeaveTypes lt ON lt.LeaveTypeID = la.LeaveTypeID
WHERE la.LeaveTypeID = 6 AND CAST(la.FromDate AS DATETIME) BETWEEN CAST(@FRSTDATE AS DATETIME) AND CAST(@FRENDATE AS DATETIME) 
AND CAST(la.ToDate AS DATETIME) BETWEEN CAST(@TOSTDATE AS DATETIME) AND CAST(@TOENDATE AS DATETIME) 
),
LOP AS
(
SELECT la.EmployeeID,la.LeaveTypeID,lt.Description,la.FromDate--,* 
FROM payroll.LeaveApplications la
LEFT JOIN payroll.LeaveTypes lt ON lt.LeaveTypeID = la.LeaveTypeID
WHERE la.IsLeaveWithoutPay = 1 
AND CAST(la.FromDate AS DATETIME) BETWEEN CAST(@FRSTDATE AS DATETIME) AND CAST(@FRENDATE AS DATETIME) 
AND CAST(la.ToDate AS DATETIME) BETWEEN CAST(@TOSTDATE AS DATETIME) AND CAST(@TOENDATE AS DATETIME) 
)
SELECT emp.EmployeeIID,emp.EmployeeCode,emp.FirstName + ' ' + IsNuLL(emp.MiddleName,'') + ' ' + emp.LastName AS EmployeeName
,COUNT(cs.EmployeeID) AS CASUAL,COUNT(sk.EmployeeID) AS SICK,COUNT(sh.EmployeeID) AS SHORT, COUNT(lp.EmployeeID) AS LOP
FROM payroll.Employees emp
LEFT JOIN CASUAL AS cs ON cs.EmployeeID = emp.EmployeeIID
LEFT JOIN SICK AS sk ON sk.EmployeeID = emp.EmployeeIID
LEFT JOIN SHORT AS sh ON sh.EmployeeID = emp.EmployeeIID
LEFT JOIN LOP AS lp ON lp.EmployeeID = emp.EmployeeIID
WHERE emp.EmployeeIID =  IIF(@EmployeeID=0,emp.EmployeeIID,@EmployeeID)
GROUP BY emp.EmployeeIID,emp.EmployeeCode,emp.FirstName,emp.MiddleName,emp.LastName,cs.FromDate,cs.EmployeeID</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeIID">
          <rd:TypeName>System.Int64</rd:TypeName>
          <DataField>EmployeeIID</DataField>
        </Field>
        <Field Name="EmployeeCode">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>EmployeeCode</DataField>
        </Field>
        <Field Name="EmployeeName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>EmployeeName</DataField>
        </Field>
        <Field Name="CASUAL">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>CASUAL</DataField>
        </Field>
        <Field Name="SICK">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>SICK</DataField>
        </Field>
        <Field Name="SHORT">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>SHORT</DataField>
        </Field>
        <Field Name="LOP">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>LOP</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Employee">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmpSearch">
            <Value>=Parameters!EmpSearch.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 0 AS EmployeeIID, 'ALL' AS EmployeeName 
WHERE LEN(ISNULL(@EmpSearch, '')) = 0

UNION ALL

SELECT emp.EmployeeIID, emp.EmployeeCode + ' - ' + emp.FirstName + ' ' + ISNULL(emp.MiddleName, '') + ' ' + emp.LastName AS EmployeeName
FROM payroll.Employees emp
WHERE LEN(ISNULL(@EmpSearch, '')) = 0

UNION ALL

SELECT emp.EmployeeIID, emp.EmployeeCode + ' - ' + emp.FirstName + ' ' + ISNULL(emp.MiddleName, '') + ' ' + emp.LastName AS EmployeeName
FROM payroll.Employees emp
WHERE (ISNULL(emp.EmployeeCode + '-', ' ') + emp.FirstName + ' ' + ISNULL(emp.MiddleName, '') + ' ' + emp.LastName LIKE '%' + ISNULL(@EmpSearch, '') + '%') 
AND LEN(ISNULL(@EmpSearch, '')) &gt; 0</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeIID">
          <rd:TypeName>System.Int64</rd:TypeName>
          <DataField>EmployeeIID</DataField>
        </Field>
        <Field Name="EmployeeName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>EmployeeName</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="LeaveTypes">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 0 AS LeaveTypeID, 'ALL' AS Description 
UNION ALL
SELECT LeaveTypeID,Description FROM payroll.LeaveTypes
UNION ALL 
SELECT 20 AS LeaveTypeID, 'LOP' AS Description </CommandText>
      </Query>
