  <DataSets>
    <DataSet Name="Books">
      <Query>
        <DataSourceName>Eduegate</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromAccNumber">
            <Value>=Parameters!FromAccNumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToAccNumber">
            <Value>=Parameters!ToAccNumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT lb.LibraryBookIID,lbm.LibraryBookMapIID,lbm.Acc_No,lbm.Call_No FROM schools.LibraryBooks lb
LEFT JOIN schools.LibraryBookMaps lbm ON lbm.LibraryBookID= lb.LibraryBookIID
WHERE lbm.LibraryBookMapIID BETWEEN @FromAccNumber AND @ToAccNumber</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
