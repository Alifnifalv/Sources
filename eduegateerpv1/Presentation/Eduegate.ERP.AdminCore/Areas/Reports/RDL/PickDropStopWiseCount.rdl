  <DataSets>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AcademicYear">
            <Value>=Parameters!AcademicYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH AllStops AS (
    SELECT 
        StopName AS Stop,
		r.RouteCode as Route
    FROM 
        schools.RouteStopMaps sr
		LEFT JOIN 
        schools.Routes r ON sr.RouteID = r.RouteID
		INNER JOIN
		schools.RouteGroups rg ON rg.RouteGroupID = r.RouteGroupID
		INNER JOIN 
        schools.AcademicYears ay ON ay.AcademicYearID = rg.AcademicYearID
    WHERE
        sr.IsActive = 1 and r.IsActive =1 AND ay.AcademicYearID = @AcademicYear
),
PickUpRouteCounts AS (
    SELECT 
        r.RouteCode AS PickupRoute,
        rs.StopName AS PickupStop,
        s.AcademicYearID,
        scl.SchoolName,
        s.AdmissionNumber AS EmpAdm,
        TRIM(ISNULL(s.FirstName,'')) + ' ' + TRIM(ISNULL(s.MiddleName,'')) + IIF(LEN(TRIM(ISNULL(s.MiddleName,'')))&gt;0,' ','') + TRIM(ISNULL(s.LastName,'')) AS Name,
        'Student' AS UserType,
		'Pickup' AS Direction
    FROM 
        schools.RouteStopMaps rs
    LEFT JOIN 
        schools.StudentRouteStopMaps sr ON rs.RouteStopMapIID = sr.PickupStopMapID
    INNER JOIN 
        schools.STUDENTS s ON s.StudentIID = sr.StudentID 
    INNER JOIN 
        schools.Schools scl ON scl.SchoolID = s.SchoolID
    INNER JOIN 
        schools.classes c ON c.ClassID = s.ClassID
    INNER JOIN 
        schools.Sections se ON se.SectionID = s.SectionID
    INNER JOIN 
        schools.AcademicYears ay ON ay.AcademicYearID = s.AcademicYearID
    LEFT JOIN 
        schools.Routes r ON sr.PickupRouteID = r.RouteID
    WHERE 
        sr.IsActive = 1 AND s.IsActive = 1 AND s.Status = 1  AND ay.AcademicYearID = @AcademicYear
    UNION ALL
    SELECT 
        r.RouteCode AS PickupRoute,
        rs.StopName AS PickupStop,
        sr.AcademicYearID,
        scl.SchoolName,
        e.EmployeeCode AS EmpAdm,
        TRIM(ISNULL(e.FirstName,'')) + ' ' + TRIM(ISNULL(e.MiddleName,'')) + IIF(LEN(TRIM(ISNULL(e.MiddleName,'')))&gt;0,' ','') + TRIM(ISNULL(e.LastName,'')) AS Name,
        'Staff' AS UserType,
		'Pickup' AS Direction
    FROM 
        schools.RouteStopMaps rs
    LEFT JOIN 
        schools.StaffRouteStopMaps sr ON rs.RouteStopMapIID = sr.PickupStopMapID
    INNER JOIN 
        payroll.employees e ON e.employeeIid = sr.StaffID
    INNER JOIN 
        schools.Schools scl ON scl.SchoolID = e.BranchID
    INNER JOIN 
        schools.AcademicYears ay ON ay.AcademicYearID = sr.AcademicYearID
    LEFT JOIN 
        schools.Routes r ON sr.PickupRouteID = r.RouteID
    WHERE 
        sr.IsActive = 1 AND e.IsActive = 1 AND ay.AcademicYearID = @AcademicYear
),
DropRouteCounts AS (
    SELECT 
        r.RouteCode AS DropRoute,
        rs.StopName AS DropStop,
        s.AcademicYearID,
        scl.SchoolName,
        s.AdmissionNumber AS EmpAdm,
        TRIM(ISNULL(s.FirstName,'')) + ' ' + TRIM(ISNULL(s.MiddleName,'')) + IIF(LEN(TRIM(ISNULL(s.MiddleName,'')))&gt;0,' ','') + TRIM(ISNULL(s.LastName,'')) AS Name,
        'Student' AS UserType,
		'Drop' AS Direction
    FROM 
        schools.RouteStopMaps rs
    LEFT JOIN 
        schools.StudentRouteStopMaps sr ON rs.RouteStopMapIID = sr.DropStopMapID
    INNER JOIN 
        schools.STUDENTS s ON s.StudentIID = sr.StudentID
    INNER JOIN 
        schools.Schools scl ON scl.SchoolID = s.SchoolID
    INNER JOIN 
        schools.classes c ON c.ClassID = s.ClassID
    INNER JOIN 
        schools.Sections se ON se.SectionID = s.SectionID
    INNER JOIN 
        schools.AcademicYears ay ON ay.AcademicYearID = s.AcademicYearID
    LEFT JOIN 
        schools.Routes r ON sr.DropStopRouteID = r.RouteID
    WHERE 
        sr.IsActive = 1  AND s.IsActive = 1 AND s.Status = 1  AND ay.AcademicYearID = @AcademicYear
    UNION ALL
    SELECT 
        r.RouteCode AS DropRoute,
        rs.StopName AS DropStop,
        sr.AcademicYearID,
        scl.SchoolName,
        e.EmployeeCode AS EmpAdm,
        TRIM(ISNULL(e.FirstName,'')) + ' ' + TRIM(ISNULL(e.MiddleName,'')) + IIF(LEN(TRIM(ISNULL(e.MiddleName,'')))&gt;0,' ','') + TRIM(ISNULL(e.LastName,'')) AS Name,
        'Staff' AS UserType,
		'Drop' AS Direction
    FROM 
        schools.RouteStopMaps rs
    LEFT JOIN 
        schools.StaffRouteStopMaps sr ON rs.RouteStopMapIID = sr.DropStopMapID
    INNER JOIN 
        payroll.employees e ON e.employeeIid = sr.StaffID
    INNER JOIN 
        schools.Schools scl ON scl.SchoolID = e.BranchID
    INNER JOIN 
        schools.AcademicYears ay ON ay.AcademicYearID = sr.AcademicYearID
    LEFT JOIN 
        schools.Routes r ON sr.DropStopRouteID = r.RouteID
    WHERE 
        sr.IsActive = 1 AND e.IsActive = 1 AND ay.AcademicYearID = @AcademicYear
)

SELECT 
    AllStops.Route,
    AllStops.Stop,
    COALESCE(SUM(CASE WHEN CombinedCounts.UserType = 'Student' AND CombinedCounts.Direction = 'Pickup' THEN 1 ELSE 0 END), 0) AS 'Student Pickup',
    COALESCE(SUM(CASE WHEN CombinedCounts.UserType = 'Staff' AND CombinedCounts.Direction = 'Pickup' THEN 1 ELSE 0 END), 0) AS 'Staff Pickup',
    COALESCE(SUM(CASE WHEN CombinedCounts.Direction = 'Pickup' THEN 1 ELSE 0 END), 0) AS 'Total Pickup',
    COALESCE(SUM(CASE WHEN CombinedCounts.UserType = 'Student' AND CombinedCounts.Direction = 'Drop' THEN 1 ELSE 0 END), 0) AS 'Student Drop',
    COALESCE(SUM(CASE WHEN CombinedCounts.UserType = 'Staff' AND CombinedCounts.Direction = 'Drop' THEN 1 ELSE 0 END), 0) AS 'Staff Drop',
    COALESCE(SUM(CASE WHEN CombinedCounts.Direction = 'Drop' THEN 1 ELSE 0 END), 0) AS 'Total Drop'
FROM 
    AllStops
LEFT JOIN (
    SELECT * FROM PickUpRouteCounts
    UNION ALL
    SELECT * FROM DropRouteCounts
) AS CombinedCounts ON AllStops.Route = CombinedCounts.PickupRoute AND AllStops.Stop = CombinedCounts.PickupStop
GROUP BY 
    AllStops.Route,
    AllStops.Stop
ORDER BY 
    AllStops.Route,
    AllStops.Stop</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
