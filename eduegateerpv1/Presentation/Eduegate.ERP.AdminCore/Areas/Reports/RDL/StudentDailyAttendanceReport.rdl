  <DataSets>
    <DataSet Name="StudentAttendance">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ADate">
            <Value>=Parameters!ADate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>--DECLARE @ADate DATETIME = '6/05/2024'

;WITH DATA AS 
(
	SELECT COUNT(stud.StudentIID) AS TotalCount, stud.SchoolID, stud.ClassID, stud.SectionID, 
	scl.SchoolName AS SchoolName, cls.ClassDescription, sct.SectionName, cls.ORDERNO
    FROM schools.Students stud
    INNER JOIN schools.Schools scl ON scl.SchoolID = stud.SchoolID
    INNER JOIN schools.Classes cls ON cls.ClassID = stud.ClassID
    INNER JOIN schools.Sections sct ON sct.SectionID = stud.SectionID
    WHERE stud.IsActive = 1 AND stud.Status = 1
    GROUP BY stud.ClassID, stud.SectionID, cls.ClassDescription, sct.SectionName, stud.SchoolID, cls.ORDERNO, scl.SchoolName

	UNION ALL

	SELECT COUNT(stud.StudentIID) AS TotalCount, stud.SchoolID, stud.ClassID * -1 AS ClassID, -1 AS SectionID, 
	scl.SchoolName AS SchoolName,cls.ClassDescription,'ALL', cls.ORDERNO -1 AS ORDERNO
    FROM schools.Students stud
    INNER JOIN schools.Classes cls ON cls.ClassID = stud.ClassID
    INNER JOIN schools.Schools scl ON scl.SchoolID = stud.SchoolID
    WHERE stud.IsActive = 1 AND stud.Status = 1
    GROUP BY stud.ClassID, stud.SchoolID, scl.SchoolName,cls.ClassDescription, cls.ORDERNO
),
PRSNT AS
(
    SELECT COUNT(stud.StudentAttendenceIID) AS PresentCount,stud.ClassID,stud.SectionID,stud.SchoolID
    FROM schools.StudentAttendences stud
    WHERE stud.AttendenceDate = @ADate AND stud.PresentStatusID = 6
    GROUP BY stud.ClassID,stud.SectionID,stud.SchoolID

	UNION ALL 

	SELECT COUNT(stud.StudentAttendenceIID) AS PresentCount,stud.ClassID * -1 AS ClassID,-1 AS SectionID,stud.SchoolID
	FROM schools.StudentAttendences stud
	WHERE stud.AttendenceDate = @ADate AND stud.PresentStatusID = 6
	GROUP BY stud.ClassID,stud.SchoolID
),
LEAVE AS
(
    SELECT COUNT(stud.StudentAttendenceIID) AS LeaveCount,stud.ClassID,stud.SectionID,stud.SchoolID 
    FROM schools.StudentAttendences stud
    WHERE stud.AttendenceDate = @ADate AND stud.PresentStatusID = 2
    GROUP BY stud.ClassID,stud.SectionID,stud.SchoolID

	UNION ALL

	SELECT COUNT(stud.StudentAttendenceIID) AS LeaveCount,ClassID * -1 AS ClassID,-1 AS SectionID,stud.SchoolID
    FROM schools.StudentAttendences stud
    WHERE stud.AttendenceDate = @ADate AND stud.PresentStatusID = 2
    GROUP BY stud.ClassID,stud.SchoolID
)
SELECT dt.SchoolID,dt.ClassID,dt.SectionID,dt.SchoolName,dt.ClassDescription,dt.SectionName,dt.ORDERNO,dt.TotalCount,
ISNULL(pr.PresentCount, 0) AS PresentCount,ISNULL(lv.LeaveCount, 0) AS LeaveCount,
CAST(ROUND(CAST(ISNULL(pr.PresentCount, 0) AS DECIMAL(10,2)) * 100.0 / CAST(dt.TotalCount AS DECIMAL(10,2)), 2) AS DECIMAL(10,2)) AS Percentage
FROM DATA dt
LEFT JOIN PRSNT pr ON pr.ClassID = dt.ClassID AND pr.SectionID = dt.SectionID AND pr.SchoolID = dt.SchoolID
LEFT JOIN LEAVE lv ON lv.ClassID = dt.ClassID AND lv.SectionID = dt.SectionID AND lv.SchoolID = dt.SchoolID
ORDER BY dt.SchoolID,dt.ORDERNO</CommandText>
