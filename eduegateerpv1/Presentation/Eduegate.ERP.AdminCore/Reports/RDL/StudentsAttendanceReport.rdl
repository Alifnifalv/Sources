    <DataSet Name="Section">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SchoolID">
            <Value>=Parameters!SchoolID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT
   0 AS SectionID
  ,'All' SectionName, 0 as SchoolID
UNION ALL
SELECT
  schools.Sections.SectionID
  ,schools.Sections.SectionName,schools.Sections.SchoolID
FROM
  schools.Sections
where SchoolID = @SchoolID</CommandText>
