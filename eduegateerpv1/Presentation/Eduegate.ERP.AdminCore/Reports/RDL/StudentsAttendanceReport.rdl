    <DataSet Name="WeekDays">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SDATE">
            <Value>=Parameters!SDATE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EDATE">
            <Value>=Parameters!EDATE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ACADEMICYEAR_IDs">
            <Value>=Parameters!ACADEMICYEAR_IDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>Select e.Description,ev.StartDate,ev.EndDate from  schools.AcademicYearCalendarEvents as ev 
inner join schools.AcademicYearCalendarEventType as e on e.AcademicYearCalendarEventTypeID = ev.AcademicYearCalendarEventTypeID
inner join schools.AcadamicCalendars as ac on ac.AcademicCalendarID = ev.AcademicCalendarID 
where ev.StartDate &gt;= @SDATE and ev.EndDate &lt;= @EDATE and ac.AcademicYearID = @ACADEMICYEAR_IDs
order by ev.StartDate asc</CommandText>
      </Query>
