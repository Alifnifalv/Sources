    <DataSet Name="Class">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SchoolID">
            <Value>=Parameters!SchoolID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT
  0 AS ClassID
  , 'All'  ClassDescription
  ,  0 AS  ShiftID
  ,  0 AS  SchoolID
  ,  0 AS  CostCenterID,0 AS Code

UNION ALL
SELECT
  schools.Classes.ClassID,
  REPLACE(REPLACE(REPLACE(REPLACE(schools.Classes.ClassDescription, '- Thumama', ''), '- Westbay', ''), '- Meshaf', ''),')', '') as ClassDescription,
  schools.Classes.ShiftID
  ,schools.Classes.SchoolID
  ,schools.Classes.CostCenterID,schools.Classes.Code
FROM
  schools.Classes
where SchoolID = @SchoolID
order by Code asc</CommandText>
      </Query>
