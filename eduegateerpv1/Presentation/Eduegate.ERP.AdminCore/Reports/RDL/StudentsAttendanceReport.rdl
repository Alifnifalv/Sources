    <DataSet Name="AcademicYear">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SchoolID">
            <Value>=Parameters!SchoolID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT
  x.AcademicYearID
  ,x.AcademicYearCode + ' (' +y.SchoolName +')' AS AcademicYearCode
  ,x.Description
  ,x.StartDate
  ,x.EndDate
  ,x.SchoolID
  ,x.IsActive
FROM
  schools.AcademicYears x INNER JOIN SCHOOLS.SCHOOLS y ON x.SchoolID=y.SchoolID
where x.SchoolID = @SchoolID
  ORDER BY YEAR(EndDate) DESC</CommandText>
      </Query>
