        <DataSourceName>EduEgate</DataSourceName>
        <CommandText>SELECT   0 AS FiscalYear_ID  ,'All' AS FiscalYear_Name  ,1 AS FiscalYear_Status   ,GETDATE() AS StartDate   ,GETDATE() AS EndDate  , YEAR(GETDATE())CurrentYear 
UNION ALL
SELECT   x.FiscalYear_ID  ,x.FiscalYear_Name  ,x.FiscalYear_Status   ,x.StartDate   ,x.EndDate  ,x.CurrentYear FROM   account.FiscalYear x 
