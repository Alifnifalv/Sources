    <DataSet Name="TransportDetail">
      <Query>
        <DataSourceName>Eduegate</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CurrentAcademicYearID">
            <Value>=Parameters!CurrentAcademicYearID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SchoolID">
            <Value>=Parameters!SchoolID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Stud_Staff">
            <Value>=Parameters!Stud_Staff.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IsOneWay">
            <Value>=Parameters!IsOneWay.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PickupRoute">
            <Value>=Parameters!PickupRoute.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DropRoute">
            <Value>=Parameters!DropRoute.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT td.STUD_STAFF_ID,td.STUD_EMP_Name,td.ADM_EMP_CODE,td.CLASS_SEC_Name,ds.DesignationName,em.WorkMobileNo,st.MobileNumber,td.PickUpStopName,td.DropStopName,td.IsOneWay,td.DateFrom,
td.DateTo,td.IsActive,td.PickUpRoute,td.DropRoute,td.AcademicYear,td.SchoolID,td.AcademicYearID,td.PickVehicleID,td.DropVehiclID,td.TransportStatus
,ISNULL(vd.VehicleCode,'') as DropBusName,ISNULL(vp.VehicleCode,'') as PickUpBusName,Student_Staff
FROM schools.TransportDetailReportView as td
left join payroll.Employees as em on td.STUD_STAFF_ID = em.EmployeeIID
left join payroll.Designations as ds on em.DesignationID = ds.DesignationID
left join schools.Students as st on td.STUD_STAFF_ID = st.StudentIID
left join mutual.vehicles vp on td.PickVehicleID = vp.VehicleIID
left join mutual.vehicles vd on td.DropVehiclID = vd.VehicleIID
where td.AcademicYearID = @CurrentAcademicYearID 
and 
td.SchoolID = @SchoolID and Student_Staff = IIF(@Stud_Staff='ALL',Student_Staff,@Stud_Staff) 
and IsOneWay = IIF(@IsOneWay='ALL',IsOneWay,@IsOneWay)
and IsOneWay = IIF(@IsOneWay='ALL',IsOneWay,@IsOneWay)
and td.PickUpRoute = IIF(@PickupRoute=0,PickUpRoute,@PickupRoute)
and td.DropRoute = IIF(@DropRoute=0,DropRoute ,@DropRoute)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
