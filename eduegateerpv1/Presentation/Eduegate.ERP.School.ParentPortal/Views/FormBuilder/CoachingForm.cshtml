<script type="text/javascript">

    $(document).ready(function () {

        $('#SubmitCoachingFormBtn').click(function () {
            $(this).prop("disabled", true);

            if ($('#IsSelectTermsAndConditions').prop("checked") == false) {

                $("#IsSelectTermsAndConditions").focus();
                $(this).prop("disabled", false);
                callToasterPlugin('error', 'Select terms and conditions');
                return false;
            }
            else if (!$("#StudentID").val() || $("#StudentID").val() == '?') {
                $("#StudentID").focus();
                $(this).prop("disabled", false);
                callToasterPlugin('error', "Select any student!");
                return false;
            }
            else if (!$("#CurriculamID").val() || $("#CurriculamID").val() == '?') {
                $("#CurriculamID").focus();
                $(this).prop("disabled", false);
                callToasterPlugin('error', "Select any curriculam!");
                return false;
            }
            else if (!$("#BloodGroup").val()) {
                $("#BloodGroup").focus();
                $(this).prop("disabled", false);
                callToasterPlugin('error', "Blood group is required!");
                return false;
            }
            else {
                $('#coachingForm').submit();
            }
        });

        if (ViewBag.IsError == true){
            callToasterPlugin('error', 'Something went wrong, try again later!');
        }

    });

</script>