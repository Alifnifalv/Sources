<link rel="shortcut icon" href="~/Design_v02/img/favicon.ico" />

<div class="page-wrapper">

    <!-- start page container -->
    <div class="page-container">

        <!-- start page content -->
        @Html.ActionLink("Click Here", "AnonymousDashbaord")
        <div class="page-content-wrapper" ng-controller="ApplicationController" ng-init="init(@Newtonsoft.Json.JsonConvert.SerializeObject(Model))">
            <div class="page-content">
                <div class="page-bar">
                    <div class="page-title-breadcrumb">
                        <div class=" pull-left">
                            <div class="page-title">Application Form</div>
                        </div>
                        <ol class="breadcrumb page-breadcrumb pull-right">
                            <li>
                                <i class="fa fa-home"></i>&nbsp;<a class="parent-item"
                                                                   href="index.html">Home</a>&nbsp;<i class="fa fa-angle-right"></i>
                            </li>
                            <li class="active">Application Form</li>
                        </ol>
                    </div>
                </div>
                <!-- basic wizard -->
                <div class="row" style="height:20px;overflow:hidden;display:none;">
                    <div class="col-sm-12">
                        <div class="card-box">
                            <div class="card-head">
                                <header>Basic Wizard nelson</header>
                            </div>
                            <div class="card-body ">
                                <div id="wizard">
                                    <h1>First Step</h1>
                                    <div>First Content</div>
                                    <h1>Second Step</h1>
                                    <div>Second Content</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- wizard with validation-->
                <div class="row">
                    <div class="col-sm-12">
                        <div class="card-box">
                           
                            <div class="card-body ">
                                <form id="example-advanced-form" action="#">
                                    <h3>Course</h3>
                                    <fieldset>
                                        <div class="row">
                                            <br />
                                            <div class="formTitle col s12">
                                                <span>School</span>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="form-group row  margin-top-20">
                                                    <label class="control-label col-md-12" for="School">Select locations of the school <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                    <div class="input-icon right">
                                                        <i class="fa"></i>
                                                        <select class="form-control validate" required="" aria-required="true" name="School" id="School" ng-options="item as item.Value for item in Schools track by item.Key" ng-model="School" autofocus tabindex="0"></select>
                                                    </div>
                                                    @Html.ValidationMessageFor(m => m.School)
                                                </div>
                                            </div>

                                            <div class="col-lg-6">
                                                <div class="form-group row  margin-top-20">
                                                    @*<select class="form-control validate" required="" aria-required="true" name="SchoolAcademicyear" id="SchoolAcademicyear"
                                                        ng-options="item as item.Value for item in AcademicYears track by item.Key" ng-model="AcademicyearID"></select>*@
                                                    <span><label class="control-label col-md-12" for="SchoolAcademicyear">Academic Year <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div> </span>
                                                    @*<div class="input-field col s8">*@
                                                    <div class="input-icon right"><i class="fa"></i><div>@Model.SchoolAcademicyear</div></div>
                                                    @*</div>*@
                                                    @Html.ValidationMessageFor(m => m.SchoolAcademicyear)
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <br />
                                            <div class="formTitle col s12">
                                                <span>Class</span>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="form-group row  margin-top-20">
                                                <label class="control-label col-md-12" for="Class">Admission looking for <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                <div class="input-icon right">
                                                    <i class="fa"></i>
                                                    <select class="form-control validate" required="" aria-required="true" name="ClassKey" id="ClassKey"
                                                            ng-options="item as item.Value for item in Classes track by item.Key" ng-model="ClassKey"></select>
                                                </div>
                                                @Html.ValidationMessageFor(m => m.ClassKey)
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="form-group row  margin-top-20">
                                                @*<select class="form-control validate" required="" aria-required="true" name="CurriculamString" id="CurriculamString"
                                                    ng-options="item as item.Value for item in SchoolSyllabus track by item.Key" ng-model="CurriculamString"></select>*@
                                                <span><label class="control-label col-md-12" for="CurriculamString">Curriculam</label> <div class="input-icon right"><i class="fa"></i></div></span>
                                                <div>@Model.CurriculamString</div>
                                            </div>
                                        </div>

                                    </fieldset>
                                    <h3>Student</h3>
                                    <fieldset>

                                        <div class="row">
                                            <br />
                                            <div class="formTitle col s12">
                                                <span>Student's Details(As per Passport)</span>
                                            </div>
                                        </div>

