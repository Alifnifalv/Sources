<!-- icons -->
<link href="~/Design_v02/fonts/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
<link href="~/Design_v02/fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="~/Design_v02/fonts/material-design-icons/material-icon.css" rel="stylesheet" type="text/css" />
<!--bootstrap -->
<link href="~/Design_v02/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
