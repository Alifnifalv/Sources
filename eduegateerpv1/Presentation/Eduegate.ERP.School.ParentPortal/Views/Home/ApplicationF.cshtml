                                                            ng-options="item as item.Value for item in Genders track by item.Key" ng-model="Gender"></select>

                                                    @Html.ValidationMessageFor(m => m.Gender)
                                                </div>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="form-group row  margin-top-20">
                                                    <label class="control-label col-md-12" for="DateOfBirthString">Date Of Birth  <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
