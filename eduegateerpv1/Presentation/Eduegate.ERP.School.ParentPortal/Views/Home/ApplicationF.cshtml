                                                    <div class="input-icon right">
                                                        <i class="fa"></i>
                                                        @Html.TextBoxFor(m => m.StudentNationalID, new { @class = "form-control validate", @type = "text", maxlength = "11", id = "StudentNationalID", value = "", onkeypress = "javascript:return isNumber(event)", @autocomplete = "off" })
                                                    </div>
                                                    @Html.ValidationMessageFor(m => m.StudentNationalID)

                                                </div>
                                            </div>
