                                                    <div class="input-icon right">
                                                        <i class="fa"></i>
                                                        @Html.TextBoxFor(m => m.VisaExpiryDateString, new
                                                        {
                                                            @class = "form-control validate datepicker validate date",
                                                            @autocomplete = "off"@*@aria_required = "false"*@})
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
