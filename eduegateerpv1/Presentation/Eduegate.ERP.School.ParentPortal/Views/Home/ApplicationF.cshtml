                                            <div class="col-lg-4">
                                                <div class="form-group row  margin-top-20">
                                                    <label class="control-label col-md-12" for="FirstName">First Name <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                    <div class="input-icon right"><i class="fa"></i>@Html.TextBoxFor(m => m.FirstName, new { @class = "form-control validate", @required = "", @aria_required = "true", ng_model = "StudentApplication.FirstName", @capitalize = "", maxlength = "20", @autocomplete = "off" })</div>
                                                    @Html.ValidationMessageFor(m => m.FirstName)

                                                </div>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="form-group row  margin-top-20">
                                                    <label class="control-label col-md-12" for="MiddleName">Middle Name</label> <div class="input-icon right"><i class="fa"></i></div><div class="input-icon right"><i class="fa"></i></div>
                                                    @Html.TextBoxFor(m => m.MiddleName, new
                                                    {
                                                        @class = "form-control validate",@*@aria_required = "false",*@ng_model = "StudentApplication.MiddleName",
@capitalize = "",
maxlength = "20",
@autocomplete = "off"
})
                                                    @Html.ValidationMessageFor(m => m.MiddleName)

                                                </div>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="form-group row  margin-top-20">
                                                    <label class="control-label col-md-12" for="LastName">Last Name  <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                    @Html.TextBoxFor(m => m.LastName, new { @class = "form-control validate", @required = "", @aria_required = "true", ng_model = "StudentApplication.LastName", @capitalize = "", maxlength = "20", @autocomplete = "off" })
                                                    @Html.ValidationMessageFor(m => m.LastName)

                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-4">
                                                <div class="form-group row  margin-top-20">
