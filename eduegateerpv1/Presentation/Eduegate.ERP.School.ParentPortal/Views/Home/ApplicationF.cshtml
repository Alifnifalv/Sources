                                                    <label class="control-label col-md-12" for="StudentNationalIDNoIssueDateString">National ID Issue Date</label>
                                                    <div class="input-icon right">
                                                        <i class="fa"></i>
                                                        @Html.TextBoxFor(m => m.StudentNationalIDNoIssueDateString, new
                                                        {
                                                            @class = "form-control validate datepicker validate maxtodayDate date",
                                                            @autocomplete = "off"@*, @aria_required = "false"*@})
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="form-group row  margin-top-20">
                                                    <label class="control-label col-md-12" for="StudentNationalIDNoExpiryDateString">National ID Expiry Date</label>
                                                    <div class="input-icon right">
                                                        <i class="fa"></i>
                                                        @Html.TextBoxFor(m => m.StudentNationalIDNoExpiryDateString, new
                                                        {
                                                       @class = "form-control validate datepicker validate date",
                                                       @autocomplete = "off"@*,  @aria_required = "false"*@})
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-12">
                                            <div class="form-group row  margin-top-20">
                                                <label class="control-label col-md-12" for="BloodGroup">BloodGroup</label>
                                                <div class="input-icon right">
                                                    <i class="fa"></i>
                                                    <select class="form-control validate" aria-required="false" name="BloodGroup" id="BloodGroup"
