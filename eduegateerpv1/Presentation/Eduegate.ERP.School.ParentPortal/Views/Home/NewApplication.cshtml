
@model Eduegate.Web.Library.School.Students.StudentApplicationViewModel
@{
    ViewBag.Title = "Attendance";
    Layout = "~/Views/Shared/_Layout_Guest.cshtml";
    Eduegate.ERP.School.ParentPortal.Common.CommonFunction commonFuction = new Eduegate.ERP.School.ParentPortal.Common.CommonFunction();
}

<!-- google font -->
<link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet" type="text/css" />
<!-- icons -->
<link href="~/Design_v02/fonts/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
<link href="~/Design_v02/fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="~/Design_v02/fonts/material-design-icons/material-icon.css" rel="stylesheet" type="text/css" />
<!--bootstrap -->
<link href="~/Design_v02/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
<!-- Material Design Lite CSS -->
<link rel="stylesheet" href="~/Design_v02/plugins/material/material.min.css">
@*<link rel="stylesheet" href="~/Design_v02/css/material_style.css">*@
<!-- steps -->
<link rel="stylesheet" href="~/Design_v02/plugins/steps/steps.css">
<!-- Theme Styles -->
<link href="~/Design_v02/css/theme/light/theme_style.css" rel="stylesheet" id="rt_style_components" type="text/css" />
<link href="~/Design_v02/css/theme/light/style.css" rel="stylesheet" type="text/css" />
<link href="~/Design_v02/css/plugins.min.css" rel="stylesheet" type="text/css" />
<link href="~/Design_v02/css/responsive.css" rel="stylesheet" type="text/css" />
<link href="~/Design_v02/css/theme/light/theme-color.css" rel="stylesheet" type="text/css" />
<!-- favicon -->
<link rel="shortcut icon" href="~/Design_v02/img/favicon.ico" />

<link href="~/Content/cropper.css" rel="stylesheet">



<!-- start page content -->
<div class="bg-light " data-aos="fade-up" ng-controller="ApplicationController" ng-init="init(@Newtonsoft.Json.JsonConvert.SerializeObject(Model))">
    <section class="home-section-wrap">
        <div class="container-custom container-xxl">
            <div class="row row-eq-height">
                <div class="d-flex flex-stack flex-wrap gap-2 mt-n1 mb-6">
                    <!--begin::Page title-->
                    <div class="page-title d-flex flex-column align-items-start me-3 py-2 py-lg-0 gap-2">
                        <!--begin::Title-->
                        <h1 class="d-flex text-dark fw-bold m-0 fs-3">Application Form</h1>
                        <!--end::Title-->
                        <!--begin::Breadcrumb-->
                        <ul class="breadcrumb breadcrumb-dot fw-semibold text-gray-600 fs-7">
                            <!--begin::Item-->
                            <li class="breadcrumb-item text-gray-600">
                                <a class="text-gray-600 text-hover-primary" href="AnonymousDashbaord">Home</a>
                            </li>
                            <!--end::Item-->
                            <!--begin::Item-->
                            <li class="breadcrumb-item text-gray-600">Application form</li>
                            <!--end::Item-->

                        </ul>
                        <!--end::Breadcrumb-->
                    </div>
                    <!--end::Page title-->

                </div>
            </div><!--pageContent-->
        </div><!--pageContainer-->
    </section>
    <section class="home-section-wrap mt-0">

        <div class="row row-eq-height">
            <div class="tab-pane fontawesome-demo p-md-sm-0">
                <div class="container-custom container-xxl d-flex flex-column-fluid  p-md-sm-0">
                    <!--begin::Content-->
                    <div class="content d-flex flex-row flex-row-fluid" id="kt_content">
                        <!--begin::Main-->
                        <div class="d-flex flex-column flex-row-fluid">
                            <!--begin::Post-->
                            <div class="flex-column-fluid">

                                <div class="">


                                    <!-- wizard with validation-->
                                    <div class="row">

                                        <div class="col-sm-12">
                                            <div class="card">

                                                <div class="card-body ">
                                                    @using (Html.BeginForm("SubmitApplication", "Home", FormMethod.Post, new { id = "StudApplication" }))
                                                    {
                                                        @Html.HiddenFor(a => a.ApplicationIID)
                                                        @Html.HiddenFor(a => a.SchoolID)
                                                        @Html.HiddenFor(a => a.RelegionID)
                                                        @*@Html.HiddenFor(a => a.ClassID)*@
                                                        <input type="hidden" value="{{ClassKey.Key}}" ng-model="Class.Key" id="ClassID" name="ClassID" />
                                                        @Html.HiddenFor(a => a.FatherMotherDetails.FatherCountryID)
                                                        @Html.HiddenFor(a => a.FatherMotherDetails.MotherCountryID)
                                                        @Html.HiddenFor(a => a.NationalityID)
                                                        @Html.HiddenFor(a => a.GenderID)
                                                        @*@Html.HiddenFor(a => a.FatherMotherDetails.FatherStudentRelationShipID)*@
                                                        //@Html.HiddenFor(a => a.MotherStudentRelationShipID)
                                                        @Html.HiddenFor(a => a.CountryofIssueID)
                                                        @Html.HiddenFor(a => a.FatherMotherDetails.FatherCountryofIssueID)
                                                        @Html.HiddenFor(a => a.FatherMotherDetails.MotherCountryofIssueID)
                                                        @Html.HiddenFor(a => a.Address.CountryID)
                                                        @Html.HiddenFor(a => a.AcademicyearID)
                                                        @Html.HiddenFor(a => a.FatherMotherDetails.CanYouVolunteerToHelpOneID)
                                                        @Html.HiddenFor(a => a.FatherMotherDetails.CanYouVolunteerToHelpTwoID)
                                                        @Html.HiddenFor(a => a.CurriculamID)
                                                        @Html.HiddenFor(a => a.SecoundLanguageID)
                                                        @Html.HiddenFor(a => a.ThridLanguageID)
                                                        @Html.HiddenFor(a => a.StudentCoutryOfBrithID)
                                                        @Html.HiddenFor(a => a.PrimaryContactID)
                                                        @*@Html.HiddenFor(a => a.CategoryID) *@
                                                        @Html.HiddenFor(a => a.CastID)
                                                        @Html.HiddenFor(a => a.CommunityID)
                                                        @Html.HiddenFor(a => a.BloodGroupID)
                                                        @Html.HiddenFor(a => a.PreviousSchoolDetails.PreviousSchoolSyllabusID)
                                                        @Html.HiddenFor(a => a.PreviousSchoolDetails.PreviousSchoolClassCompletedID)
                                                        @Html.HiddenFor(a => a.StudentPassportDetailNoID)
                                                        @Html.HiddenFor(a => a.FatherMotherDetails.FatherPassportDetailNoID)
                                                        @Html.HiddenFor(a => a.FatherMotherDetails.MotherPassportDetailNoID)
                                                        @Html.HiddenFor(a => a.StudentVisaDetailNoID)
                                                        @*@Html.HiddenFor(a => a.FatherVisaDetailNoID)
                                                            @Html.HiddenFor(a => a.MotherVisaDetailNoID) *@
                                                        @Html.HiddenFor(a => a.CreatedBy)
                                                        @Html.HiddenFor(a => a.CreatedDate)
                                                        @Html.HiddenFor(a => a.ApplicationNumber)
                                                        @Html.HiddenFor(a => a.ProspectusNumber)
                                                        @Html.HiddenFor(a => a.LoginID)

                                                        //Convert Invalide Date
                                                        //Model.SchoolAcademicYear=
                                                        Model.PassportNoIssueString = commonFuction.ConvertInvalideDate(Model.PassportNoIssueString);
                                                        Model.DateOfBirthString = commonFuction.ConvertInvalideDate(Model.DateOfBirthString);
                                                        Model.PassportNoExpiryString = commonFuction.ConvertInvalideDate(Model.PassportNoExpiryString);
                                                        Model.VisaExpiryDateString = commonFuction.ConvertInvalideDate(Model.VisaExpiryDateString);
                                                        Model.StudentNationalIDNoIssueDateString = commonFuction.ConvertInvalideDate(Model.StudentNationalIDNoIssueDateString);
                                                        Model.StudentNationalIDNoExpiryDateString = commonFuction.ConvertInvalideDate(Model.StudentNationalIDNoExpiryDateString);
                                                        Model.FatherMotherDetails.FatherPassportNoIssueString = commonFuction.ConvertInvalideDate(Model.FatherMotherDetails.FatherPassportNoIssueString);
                                                        Model.FatherMotherDetails.FatherPassportNoExpiryString = commonFuction.ConvertInvalideDate(Model.FatherMotherDetails.FatherPassportNoExpiryString);
                                                        Model.FatherMotherDetails.FatherNationalDNoIssueDateString = commonFuction.ConvertInvalideDate(Model.FatherMotherDetails.FatherNationalDNoIssueDateString);
                                                        Model.FatherMotherDetails.FatherNationalDNoExpiryDateString = commonFuction.ConvertInvalideDate(Model.FatherMotherDetails.FatherNationalDNoExpiryDateString);
                                                        Model.FatherMotherDetails.MotherPassportNoIssueString = commonFuction.ConvertInvalideDate(Model.FatherMotherDetails.MotherPassportNoIssueString);
                                                        Model.FatherMotherDetails.MotherPassportNoExpiryString = commonFuction.ConvertInvalideDate(Model.FatherMotherDetails.MotherPassportNoExpiryString);
                                                        Model.FatherMotherDetails.MotherNationalDNoIssueDateString = commonFuction.ConvertInvalideDate(Model.FatherMotherDetails.MotherNationalDNoIssueDateString);
                                                        Model.FatherMotherDetails.MotherNationaIDNoExpiryDateString = commonFuction.ConvertInvalideDate(Model.FatherMotherDetails.MotherNationaIDNoExpiryDateString);

                                                        //Guardian Details
                                                        @Html.HiddenFor(a => a.GuardianDetails.CountryofIssueID)
                                                        @Html.HiddenFor(a => a.GuardianDetails.GuardianNationalityID)
                                                        @Html.HiddenFor(a => a.GuardianDetails.GuardianPassportDetailNoID)
                                                        @Html.HiddenFor(a => a.GuardianDetails.GuardianStudentRelationShipID)
                                                        Model.GuardianDetails.GuardianNationalIDNoIssueDateString = commonFuction.ConvertInvalideDate(Model.GuardianDetails.GuardianNationalIDNoIssueDateString);
                                                        Model.GuardianDetails.GuardianNationalIDNoExpiryDateString = commonFuction.ConvertInvalideDate(Model.GuardianDetails.GuardianNationalIDNoExpiryDateString);
                                                        Model.GuardianDetails.GuardianPassportNoIssueString = commonFuction.ConvertInvalideDate(Model.GuardianDetails.GuardianPassportNoIssueString);
                                                        Model.GuardianDetails.GuardianPassportNoExpiryString = commonFuction.ConvertInvalideDate(Model.GuardianDetails.GuardianPassportNoExpiryString);
                                                        //End Convert Invalide Date

                                                        //DocumentUploads
                                                        <input type="hidden" ng-value="ResetBirthCertificate" name="ResetBirthCertificate" />
                                                        <input type="hidden" ng-value="ResetStudentPassportReference" name="ResetStudentPassportReference" />
                                                        @*<input type="hidden" ng-value="DocumentsUpload[0].StudentPassportReferenceID" name="DocumentsUpload[0].StudentPassportReferenceID" />*@
                                                        <input type="hidden" ng-value="ResetTC" name="ResetTC" />
                                                        <input type="hidden" ng-value="ResetFatherQID" name="ResetFatherQID" />
                                                        <input type="hidden" ng-value="ResetMotherQID" name="ResetMotherQID" />
                                                        <input type="hidden" ng-value="ResetStudentQID" name="ResetStudentQID" />


                                                        <h3>Course</h3>
                                                        <fieldset>
                                                            <div class="row">
                                                                <br />
                                                                <div class="formTitle col s12">

                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="School">Select locations of the school <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" required="" aria-required="true" name="School" id="School" ng-options="item as item.Value for item in Schools track by item.Key" ng-model="School" autofocus tabindex="0" ng-change="SchoolChanges($select)"></select>
                                                                        @Html.ValidationMessageFor(m => m.School)
                                                                    </div>
                                                                </div>

                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="SchoolAcademicyear">Academic Year <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" required="" aria-required="true" name="SchoolAcademicyear" id="SchoolAcademicyear"
                                                                                ng-options="item as item.Value for item in AcademicYears track by item.Key" ng-model="SchoolAcademicyear"></select>
                                                                        @Html.ValidationMessageFor(m => m.SchoolAcademicyear)
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="CurriculamString">Curriculam<span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" required="" aria-required="true" name="CurriculamString" id="CurriculamString"
                                                                                ng-options="item as item.Value for item in SchoolSyllabus track by item.Key" ng-model="CurriculamString"></select>
                                                                    </div>
                                                                    @Html.ValidationMessageFor(m => m.CurriculamString)
                                                                </div>
                                                            </div>

                                                            <div class="row">
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="Class">Admission looking for <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" required="" aria-required="true" name="ClassKey" id="ClassKey"
                                                                                ng-options="item as item.Value for item in Classes track by item.Key" ng-model="ClassKey" ng-change="onclassDataChange()"></select>

                                                                        @Html.ValidationMessageFor(m => m.ClassKey)
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4" ng-if="onStreams">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="StreamGroup">Stream Group</label>
                                                                        <select class="form-control validate" required="" aria-required="true" name="StreamGroup" id="StreamGroup"
                                                                                ng-options="item as item.Value for item in StreamGroups track by item.Key" ng-model="StreamGroup" ng-change="StreamGroupChanges($select)"></select>
                                                                    </div>
                                                                    @Html.ValidationMessageFor(m => m.StreamGroup)
                                                                </div>
                                                                <div class="col-lg-4" ng-if="onStreams">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="Stream">Stream</label>
                                                                        <select class="form-control validate" required="" aria-required="true" name="Stream" id="Stream"
                                                                                ng-options="item as item.Value for item in Streams track by item.Key" ng-model="Stream"></select>
                                                                    </div>
                                                                    @Html.ValidationMessageFor(m => m.Stream)
                                                                </div>

                                                            </div>
                                                        </fieldset>
                                                        <h3>Student</h3>
                                                        <fieldset>

                                                            <div class="row">
                                                                <br />
                                                                <div class="formTitle col s12">
                                                                    <span>Student's Details(As per Passport)</span>
                                                                </div>
                                                            </div>

                                                            <div class="row">
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="FirstName">First Name <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                                        @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control validate", @required = "", @aria_required = "true", ng_model = "StudentApplication.FirstName", @capitalize = "", maxlength = "20", @autocomplete = "off" })
                                                                        @Html.ValidationMessageFor(m => m.FirstName)
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="MiddleName">Middle Name<span class="required"></span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                                        @Html.TextBoxFor(m => m.MiddleName, new
                                                                   {
                                                                       @class = "form-control validate",@*@aria_required = "false",*@ng_model = "StudentApplication.MiddleName",
@capitalize = "",
maxlength = "20",
@autocomplete = "off"
})
                                                                        @Html.ValidationMessageFor(m => m.MiddleName)
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="LastName">Last Name  <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                                        @Html.TextBoxFor(m => m.LastName, new { @class = "form-control validate", @required = "", @aria_required = "true", ng_model = "StudentApplication.LastName", @capitalize = "", maxlength = "20", @autocomplete = "off" })
                                                                        @Html.ValidationMessageFor(m => m.LastName)

                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="Gender">Gender  <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" required="" aria-required="true" name="Gender" id="Gender"
                                                                                ng-options="item as item.Value for item in Genders track by item.Key" ng-model="Gender"></select>

                                                                        @Html.ValidationMessageFor(m => m.Gender)
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="DateOfBirthString">Date Of Birth  <span class="required"> * </span></label> <div class="input-icon right"><i class="fa"></i></div>
                                                                        @*<input type="date" id="DateOfBirthString" name="DateOfBirthString" class="form-control" required autocomplete="off" />*@
                                                                        @Html.TextBoxFor(m => m.DateOfBirthString, new { @class = "form-control validate datepicker validate dateofbirth date", id = "DateOfBirthString", @required = "", @aria_required = "true", @autocomplete = "off", @onchange = "onDateOfBirthChange()" })
                                                                        @Html.ValidationMessageFor(m => m.DateOfBirthString)

                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="StudentCoutryOfBrith">Country Of Birth  <span class="required"> * </span></label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" required="" aria-required="true" name="StudentCoutryOfBrith" id="StudentCoutryOfBrith"
                                                                                ng-options="item as item.Value for item in Countries track by item.Key" ng-model="StudentCoutryOfBrith"></select>
                                                                        @Html.ValidationMessageFor(m => m.StudentCoutryOfBrith)
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="Relegion">Religion</label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" aria-required="false" name="Relegion" id="Relegion"
                                                                                ng-options="item as item.Value for item in Relegions track by item.Key" ng-model="Relegion" ng-change="RelegionChanges($select)"></select>
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="Cast">Caste</label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" aria-required="false" name="Cast" id="Cast"
                                                                                ng-options="item as item.Value for item in Casts track by item.Key" ng-model="Cast"></select>
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="Community">Community</label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" aria-required="false" name="Community" id="Community"
                                                                                ng-options="item as item.Value for item in Communitys track by item.Key" ng-model="Community"></select>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="Nationality">Nationality <span class="required"> * </span></label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" required="" aria-required="true" name="Nationality" id="Nationality"
                                                                                ng-options="item as item.Value for item in Nationalities track by item.Key" ng-model="Nationality"></select>
                                                                        @Html.ValidationMessageFor(m => m.Nationality)
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="AdhaarCardNo">Adhaar No.<span class="required"></span></label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        @Html.TextBoxFor(m => m.AdhaarCardNo, new
                                                                   {
                                                                       @class = "form-control validate",
                                                                       @type = "text",
                                                                       maxlength = "12",
                                                                       id = "AdhaarCardNo",
                                                                       value = "",
                                                                       onkeypress = "javascript:return isNumber(event)",
                                                                       @autocomplete = "off",
                                                                       disabled = "disabled"
                                                                   })
                                                                        @Html.ValidationMessageFor(m => m.AdhaarCardNo)
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="StudentPassportNo">Passport No.  <span class="required"> * </span></label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        @Html.TextBoxFor(m => m.StudentPassportNo, new { @class = "form-control validate", @required = "", @aria_required = "true", maxlength = "12", @autocomplete = "off" })
                                                                        @Html.ValidationMessageFor(m => m.StudentPassportNo)
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="PassportNoIssueString">Passport Issue Date  <span class="required"> * </span></label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        @Html.TextBoxFor(m => m.PassportNoIssueString, new { @class = "form-control date", @required = "", @aria_required = "true", @autocomplete = "off" })
                                                                        @Html.ValidationMessageFor(m => m.PassportNoIssueString)
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="PassportNoExpiryString">Passport Expiry Date  <span class="required"> * </span></label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        @*<input type="date" id="PassportNoExpiryString" name="PassportNoExpiryString" class = "form-control" required autocomplete="off"/>*@
                                                                        @Html.TextBoxFor(m => m.PassportNoExpiryString, new { @class = "form-control date", @required = "", @aria_required = "true", @autocomplete = "off" })
                                                                        @Html.ValidationMessageFor(m => m.PassportNoExpiryString)
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20">
                                                                        <label class="control-label col-md-12" for="StudentCountryofIssue"> Country Of Issue  <span class="required"> * </span></label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        <select class="form-control validate" required="" aria-required="true" name="StudentCountryofIssue" id="StudentCountryofIssue"
                                                                                ng-options="item as item.Value for item in Countries track by item.Key" ng-model="StudentCountryofIssue"></select>

                                                                        @Html.ValidationMessageFor(m => m.StudentCountryofIssue)
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-lg-4">
                                                                    <div class="form-group row  margin-top-20 padding-b-5">
                                                                        <label class="control-label col-md-12" for="StudentNationalID">National ID  <span class="required"> * </span></label>
                                                                        <div class="input-icon right"><i class="fa"></i></div>
                                                                        @Html.TextBoxFor(m => m.StudentNationalID, new { @class = "form-control validate", @required = "", @aria_required = "true", @type = "text", maxlength = "11", id = "StudentNationalID", value = "", onkeypress = "javascript:return isNumber(event)", @autocomplete = "off" })

                                                                        @Html.ValidationMessageFor(m => m.StudentNationalID)
                                                                    </div>
                                                                </div>
