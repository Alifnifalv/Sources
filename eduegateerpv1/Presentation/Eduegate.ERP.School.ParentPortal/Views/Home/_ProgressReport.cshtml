
<style>

    .pr-row-head {
        text-align: left;
    }
</style>
<div class="card card-box" ng-repeat="exam in StudentMarkList">
    <div class="card-head bg-gray">
        <header ng-bind="exam.ExamName">-</header>
        <div class="tools">
            <a class="t-collapse btn-color fa fa-chevron-down" style="color:white;"
               href="javascript:;"></a>

        </div>
    </div>
    <div class="card-body ">
        <table border="1" style="width:100%" class="table table-striped table-bordered table-hover table-checkable order-column valign-middle">
            <tr>
                <th>Subject</th>
                <th>Type</th>
                <th>Min Mark</th>
                <th>Max Mark</th>
                <th>Mark</th>
                <th>Grade</th>
                <th>Passed/Failed</th>
            </tr>
            <tr ng-repeat="result in exam.MarkList">
                <td ng-bind="result.SubjectName"></td>
                <td ng-bind="result.ExamTypeName"></td>
                <td ng-bind="result.MinimumMarks"></td>
                <td ng-bind="result.MaximumMarks"></td>
                <td ng-bind="result.Mark"></td>
                <td ng-bind="result.GradeName"></td>
                <td><span style="color:green" ng-if="result.IsPassed">Passed</span><span style="color:red" ng-if="!result.IsPassed">Failed</span></td>
            </tr>
            <tr>
                <td colspan="7" ng-if="exam.MarkList.length==0"><strong>-No Result-</strong></td>
            </tr>
        </table>

    </div>
</div>


