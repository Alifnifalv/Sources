@using Eduegate.Services.Contracts.Enums;
@model Eduegate.Framework.PageRendererEngine.ViewModels.BoilerPlateInfoViewModel

@{
    Random rnd = new Random();
    var windowName = "window_" + rnd.Next().ToString();
}

<div id="@windowName" tempalte="_PieChart" ng-controller="BoilerplatePieChartController" ng-init="childInit(@Newtonsoft.Json.JsonConvert.SerializeObject(Model),'@windowName')">

    <div id="piechart" class="col-md-4 charts-grids widget">
        <h4 class="title">@(Model.GetParameterValue<string>("Title"))</h4>
        <canvas id="pie" height="300" width="400"> </canvas>
    </div>
</div>

