                                <path d="M0.999878 8.99967L0.999878 9.99969L13.4999 9.99969L13.4999 8.99967L0.999878 8.99967Z" fill="white" />
                                <path d="M16.5002 11.5001L16.5002 12.5002L22.501 12.5002L22.501 11.5001L16.5002 11.5001Z" fill="white" />
                                <path d="M0.500366 11.5002L0.500366 12.5002L11.5003 12.5002L11.5003 11.5002L0.500366 11.5002Z" fill="white" />
                                <path d="M2.50012 17.4998L2.50012 18.4999L9.00089 18.4999L9.00089 17.4998L2.50012 17.4998Z" fill="white" />
                                <path d="M2.50012 14.9999L2.50012 16L6.50062 16L6.50062 14.9999L2.50012 14.9999Z" fill="white" />
                                <path d="M16.5002 14.9995L16.5002 17.9999C16.5002 18.2759 16.7237 18.4999 17.0003 18.4999L20.0006 18.4999C20.2772 18.4999 20.5007 18.2759 20.5007 17.9999L20.5007 14.9995C20.5007 14.7235 20.2772 14.4995 20.0006 14.4995L17.0003 14.4995C16.7237 14.4995 16.5002 14.7235 16.5002 14.9995ZM19.5006 15.4996L19.5006 17.4998L17.5004 17.4998L17.5004 15.4996L19.5006 15.4996Z" fill="white" />
                            </svg>
                        </span>
                        Proceed to Pay
                    </button>
                </div>
            </div>
        </div>
    </div>

</div>
