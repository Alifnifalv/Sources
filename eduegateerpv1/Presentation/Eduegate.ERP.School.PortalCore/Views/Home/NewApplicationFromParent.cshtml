                                                                                @Html.ValidationMessageFor(m => m.StreamKey)
                                                                            </td>
                                                                            <td>
                                                                                <ul>
                                                                                    <li ng-repeat="CompulosorySubjs in stream.CompulsorySubjects">{{CompulosorySubjs.Value}}</li>
                                                                                </ul>
                                                                            </td>
