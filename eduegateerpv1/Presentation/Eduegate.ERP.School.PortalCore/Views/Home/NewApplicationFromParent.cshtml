                                                                    @Html.TextBoxFor(m => m.MiddleName, new
                                                               {
                                                                   @class = "form-control validate",@*@aria_required = "false",*@ng_model = "StudentApplication.MiddleName",
@capitalize = "",
maxlength = "20",
@autocomplete = "off"
})
