                                                                                    <li ng-repeat="OptionalSubjs in stream.OptionalSubjects track by $index" ng-model="OptionalSubjs.Key">
                                                                                        <input type="radio" id="OptionalSubjectID" name="OptionalSubjectID" ng-value="OptionalSubjs.Key" />
                                                                                        {{OptionalSubjs.Value}}
