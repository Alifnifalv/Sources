        <script src="~/ToasterPlugin/simple-notify.min.js" asp-append-version="true"></script>
        <script>

            function callToasterPlugin(status, title) {
                new Notify({
                    status: status,
                    title: title,
                    effect: 'fade',
                    speed: 300,
                    customClass: null,
                    customIcon: null,
                    showIcon: true,
                    showCloseButton: true,
                    autoclose: true,
                    autotimeout: 3000,
                    gap: 20,
                    distance: 20,
                    type: 1,
                    position: 'center'
                })
            }

        </script>
    </div>
