
@{
    ViewBag.Title = "Results";
    var ExamsResults = ViewBag.ExamResults;
}

<div id="OnlineExamQuestions" ng-controller="ExamResultController" ng-init="Init(@Newtonsoft.Json.JsonConvert.SerializeObject(ExamsResults))">

    <div class="content-part">
        <div class="card">
            <div class="card-body">
                <main class="col-md-12">
                    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
                        <h1 class="h2">Exams Result</h1>
                    </div>
                </main>
            </div>
        </div>

        <div class="card">
            <div class="card-body">
                <table class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <th>SL/NO</th>
                            <th>Exam Name</th>
                            <th>Mark</th>
                            <th>Remarks</th>
                            <th>School Name</th>
                            <th>Academic Year</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="result in ExamResultDatas">
                            <td>{{$index + 1}}</td>
                            <td>{{result.OnlineExamName}}</td>
                            <td>{{result.Marks}}</td>
                            <td>{{result.Remarks}}</td>
                            <td>{{result.SchoolName}}</td>
                            <td>{{result.AcademicYear}}</td>
                            @*<td class="btn btn-primary" style="color: white;" ng-click="ViewResult()">View Result</td>*@
                        </tr>
                    </tbody>
                </table>
                <div class="alert alert-warning text-center" role="alert" ng-if="ExamResultDatas.length == 0 || !ExamResultDatas">
                    <span>No result found, please try again later.</span>
                </div>
            </div>
        </div>

    </div>

</div>