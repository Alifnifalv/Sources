<!DOCTYPE html>
<html ng-app="Eduegate.OnlineExam.Portal">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Eduegate.OnlineExam</title>
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/Site.css" />
    <link href="~/Content/dashboard/dashboard.css" rel="stylesheet" />
    <link href="~/Content/dashboard/docs.css" rel="stylesheet" />
    <link href="~/Content/dashboard/style.bundle.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="d-flex flex-column flex-root">
        <div class="docs-page d-flex flex-row flex-column-fluid">
            <div id="kt_docs_aside" class="docs-aside" data-kt-drawer="true" data-kt-drawer-name="aside" data-kt-drawer-activate="{default: true, lg: false}" data-kt-drawer-overlay="true" data-kt-drawer-width="{default:'200px', '300px': '250px'}" data-kt-drawer-direction="start" data-kt-drawer-toggle="#kt_docs_aside_toggle" style="">
                <!--begin::Logo-->
                <div class="docs-aside-logo flex-column-auto h-75px">
                    <a>
                        <img alt="Logo" src="~/img/PODARLOGO-01.png" class="h-35px">
                    </a>
                </div>
                <div class="docs-aside-menu flex-column-fluid">
                    <div class="hover-scroll-overlay-y mt-5 mb-5 mt-lg-0 mb-lg-5 pe-lg-n2 me-lg-2" id="kt_docs_aside_menu_wrapper" data-kt-scroll="true" data-kt-scroll-activate="{default: false, lg: true}" data-kt-scroll-height="auto" data-kt-scroll-dependencies="#kt_docs_aside_logo" data-kt-scroll-wrappers="#kt_docs_aside_menu" data-kt-scroll-offset="10px" style="height: 668px;">
                        <!--begin::Menu-->
                        <div class="menu menu-column menu-title-gray-800 menu-state-title-primary menu-state-icon-primary menu-state-bullet-primary menu-arrow-gray-500" id="#kt_docs_aside_menu" data-kt-menu="true">
                            <div class="menu-item">
                                <h4 class="menu-content text-muted mb-0 fs-7 text-uppercase">Getting Started</h4>
                            </div>
                            <div class="menu-item">
                                <a class="menu-link py-2" href="~/">
                                    <span class="menu-title">Home</span>
                                </a>
                            </div>
                            <div class="menu-item">
                                <a class="menu-link py-2" href="~/home/exam">
                                    <span class="menu-title">Exam</span>
                                </a>
                            </div>
                            <div class="menu-item">
                                <a class="menu-link py-2" href="~/home/result">
                                    <span class="menu-title">Result</span>
                                </a>
                            </div>
                            <div class="menu-item">
                                <a class="menu-link py-2" href="~/account/signout">
                                    <span class="menu-title">Sign out</span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="docs-wrapper d-flex flex-column flex-row-fluid">
                <div class="docs-header align-items-stretch mb-2 mb-lg-10">
                    <div class="container">
                        <div class="d-flex align-items-stretch justify-content-between py-3 h-75px">
                            <!--begin::Aside toggle-->
                            <div class="d-flex align-items-center d-lg-none ms-n2 me-1" title="Show aside menu">
                                <div class="btn btn-icon btn-flex btn-active-color-primary" id="kt_docs_aside_toggle">
                                    <span class="svg-icon svg-icon-1 mt-1">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                            <path d="M21 7H3C2.4 7 2 6.6 2 6V4C2 3.4 2.4 3 3 3H21C21.6 3 22 3.4 22 4V6C22 6.6 21.6 7 21 7Z" fill="currentColor"></path>
                                            <path opacity="0.3" d="M21 14H3C2.4 14 2 13.6 2 13V11C2 10.4 2.4 10 3 10H21C21.6 10 22 10.4 22 11V13C22 13.6 21.6 14 21 14ZM22 20V18C22 17.4 21.6 17 21 17H3C2.4 17 2 17.4 2 18V20C2 20.6 2.4 21 3 21H21C21.6 21 22 20.6 22 20Z" fill="currentColor"></path>
                                        </svg>
                                    </span>
                                </div>
                            </div>
                            <div class="d-flex d-lg-none align-items-center flex-grow-1 flex-lg-grow-0 me-3 me-lg-15">
                                <a>
                                    <img alt="Logo" src="/start-html-pro/assets/media/logos/logo-default.svg" class="h-35px">
                                </a>
                            </div>
                            <div class="d-flex align-items-center justify-content-between flex-lg-grow-1">
                                <div class="d-flex align-items-center">
                                    <div class="docs-page-title d-flex flex-column flex-lg-row align-items-lg-center py-5 mb-lg-0">
                                        <h1 class="d-flex align-items-center text-dark my-1 fs-4">
                                            Exam
                                        </h1>
                                    </div>
                                </div>
                                <div class="d-flex align-items-center">
                                </div>
                            </div>
                        </div>
                        <div class="border-gray-300 border-bottom"></div>
                    </div>
                </div>
                <div class="docs-content d-flex flex-column flex-column-fluid">
                    <div class="container d-flex flex-column flex-lg-row">
                        <div class="card card-docs flex-row-fluid mb-2">
                            <div class="card-body fs-6 py-15 px-10 py-lg-15 px-lg-15 text-gray-700">
                                @RenderBody()
                            </div>
                        </div>
                        <div class="ms-7 flex-row-auto w-lg-200px">
                        </div>
                    </div>
                </div>
                <div class="py-4 d-flex flex-lg-column py-6">
                    <div class="container d-flex flex-column flex-md-row align-items-center justify-content-between">
                        <div class="text-dark order-2 order-md-1">
                            <span class="text-muted fw-bold me-1">2022©</span>
                            <a class="text-gray-800 text-hover-primary">pearl school</a>
                        </div>
                        <ul class="menu menu-gray-600 menu-hover-primary fw-bold order-1 align-items-center mb-3 mb-md-0">
                            <li class="menu-item">
                                <a class="menu-link px-3">
                                    @*<img class="h-20px">*@
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/appmodels")
    <script src="~/Scripts/bootstrap.bundle.min.js"></script>
    <script src="~/Scripts/site.js" asp-append-version="true"></script>
    @*<script src="~/Content/dashboard/dashboard.js"></script>*@
    <script src="~/Scripts/bootstrap.bundle.min.js"></script>
    @RenderSection("Scripts", required: false)

    <link rel="stylesheet" href="~/ToasterPlugin/simple-notify.min.css" />
    <script src="~/ToasterPlugin/simple-notify.min.js"></script>
    <script>
        utility.myHost = "@string.Format("{0}://{1}{2}/", Request.Url.Scheme, Request.Url.Authority, HttpRuntime.AppDomainAppVirtualPath)";

        function callToasterPlugin(status, title, timeInSec) {
            new Notify({
                status: status,
                title: title,
                effect: 'fade',
                speed: 300,
                customClass: null,
                customIcon: null,
                showIcon: true,
                showCloseButton: true,
                autoclose: true,
                autotimeout: timeInSec != null ? timeInSec : 3000,
                gap: 20,
                distance: 20,
                type: 1,
                position: 'center'
            })
        }
    </script>
</body>
</html>
