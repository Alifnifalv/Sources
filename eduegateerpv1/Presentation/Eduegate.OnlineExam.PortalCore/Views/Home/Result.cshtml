@{
    ViewBag.Title = "Results";
    var ExamsResults = ViewBag.ExamResults;
}

<div id="OnlineExamQuestions" ng-controller="ExamResultController" ng-init="Init(@Newtonsoft.Json.JsonConvert.SerializeObject(ExamsResults))">

    @* <div class="content-part">
    <div class="card">
    <div class="card-body">
    <main class="col-md-12">
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Exams Result</h1>
    </div>
    </main>
    </div>
    </div> *@

    <div class="container my-5">
        <div class="card w-75 p-5 rounded-4">
            <div class="heading1">
                Exam Results
            </div>
            <br>
            <br>
            <div class="card-body p-0" ng-if="ExamResultDatas.length != 0 || ExamResultDatas">
                <table class="table mb-0 table table-striped">
                    <thead>
                        <tr class="fw-revert">
                            <th scope="col">Sl./No.</th>
                            <th scope="col">Exam Name</th>
                            <th scope="col">Mark</th>
                            <th scope="col">Remarks</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="result in ExamResultDatas" class="">
                            <td>{{$index + 1}} .</td>
                            <td>{{result.OnlineExam.Value}}</td>
                            <td>{{result.MarksObtained}}</td>
                            <td>{{result.Remarks}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="alert alert-warning text-center" role="alert" ng-if="ExamResultDatas.length == 0 || !ExamResultDatas">
                <span>No result found, please try again later.</span>
            </div>
        </div>
    </div>

</div>

@* </div> *@