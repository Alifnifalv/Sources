            <div class="profileTabHeader p-2">
                <svg class="profilePhoto" width="41" height="41" viewBox="0 0 41 41" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <circle id="Ellipse" cx="20.5" cy="20.5" r="20.5" fill="url(#pattern0_145_139)" />
                    <defs>
                        <pattern id="pattern0_145_139" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_145_139" transform="translate(-0.00251256) scale(0.00502513)" />
                        </pattern>
                        <image id="image0_145_139" width="200" height="199" xlink:href="data:image/png;base64," />
                    </defs>
                </svg>
                <div class="profileInfo">
                    <label class="pt-4">{{HeaderInfoModel.LoginEmailID}}</label>
                    <p class="headerMail"><span ng-if="HeaderInfoModel.LoginUserID">(<span ng-bind="HeaderInfoModel.LoginUserID"></span>)</span></p>
                </div>
            </div>
        </div>
        <div class="sidebar">
            <div class="d-flex align-items-center">
                <img class="eduegate-icon me-2" src="~/Clients/@client/Images/main_logo.png" alt="Logo" />
                <div class="d-flex flex-column">
                    <div class="heading1">Exam</div>
                    <div class="heading2">Portal</div>
                </div>
            </div>
            <br>
            <br>
            <br>
            <div class="pt-10">
                <a class="menu-link" href="~/home/home">
                    <img class="dashboard-icon" src="~/Clients/@client/Images/dashboard.svg" />
                    &ensp; Dashboard
                </a>
                <a class="menu-link" href="~/home/exam">
                    <img class="dashboard-icon" src="~/Clients/@client/Images/exam.svg" />
                    &ensp; Exam
                </a>
                <a class="menu-link" href="~/home/result">
                    <img class="dashboard-icon" src="~/Clients/@client/Images/result.svg" />
                    &ensp; Result
                </a>

                <a class="menu-link" href="~/account/signout">
                    <img class="dashboard-icon" src="~/Clients/@client/Images/signout.svg" />
                    &ensp; Log out
                </a>
            </div>
        </div>
        <div class="content">
            @RenderBody()
        </div>
        <div class="footer">
            &copy; @clientFullName
        </div>

        @* @RenderSection("Scripts", required: false) *@
    </div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.bundle.min.js"></script>
    <script src="~/Scripts/site.js"></script>
    <script src="~/ToasterPlugin/simple-notify.min.js"></script>

    @RenderSection("Scripts", required: false)

    <script>
        function callToasterPlugin(status, title, timeInSec) {
            new Notify({
                status: status,
                title: title,
                effect: 'fade',
                speed: 300,
                customClass: null,
                customIcon: null,
                showIcon: true,
                showCloseButton: true,
                autoclose: true,
                autotimeout: timeInSec != null ? timeInSec : 3000,
                gap: 20,
                distance: 20,
                type: 1,
                position: 'center'
            });
        }
    </script>
</body>
</html>

