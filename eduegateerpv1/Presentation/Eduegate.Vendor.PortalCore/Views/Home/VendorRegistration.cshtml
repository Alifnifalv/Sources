                    <div class="row">
                        <div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="CRNumber">CR Number<span class="required"> </span></label>
                                <input type="text" id="CRNumber" ng-model="Vendor.BusinessDetail.VendorCR" name="CR Number" required>
