@model Eduegate.Web.Library.ViewModels.SupplierViewModel

@{
    ViewData["Title"] = "My Profile";
}

<div style="text-align:center;" class="pt-1">
    @await Html.PartialAsync("_ToastMessage")
</div>
<h3 class="pb-5">
