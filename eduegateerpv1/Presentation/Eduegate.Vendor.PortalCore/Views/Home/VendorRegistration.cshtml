                        </div>
                        <div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label for="CRStartDate">CR Start Date</label>
                                <input type="text" autocomplete="off" ng-model="Vendor.BusinessDetail.CRStartDateString" datetime-picker />
                            </div>
                        </div>
                        <div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="CRExpiryDate">CR Expiry Date</label>
                                <input type="text" ng-model="Vendor.BusinessDetail.CRExpiryDateString" id="CRExpiryDate" name="CRExpiryDate" datetime-picker ng-change="validateDates('CRExpiryDateString')">
                            </div>
                        </div>

                    </div>

