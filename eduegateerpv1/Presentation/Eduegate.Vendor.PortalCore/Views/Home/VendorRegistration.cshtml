    </svg> My Account
</h3>
<div id="MyProfileForm" ng-controller="VendorController" ng-init="Init('VendorRegistration')">
    <div class="navTabs">
        <ul class="nav nav-tabs pb-2 pt-2">
            <li class="nav-item px-2">
                <a class="nav-link active" data-bs-toggle="tab" href="#tab1">Vendor Information</a>
            </li>
            <li class="nav-item px-2">
                <a class="nav-link" data-bs-toggle="tab" href="#tab2">Business Details</a>
            </li>
            <li class="nav-item px-2">
                <a class="nav-link" data-bs-toggle="tab" href="#tab3">Financial Information</a>
            </li>
            <li class="nav-item px-2">
                <a class="nav-link" data-bs-toggle="tab" href="#tab4">Product/Service Information</a>
            </li>
            <li class="nav-item px-2">
                <a class="nav-link" data-bs-toggle="tab" href="#tab5">Compliance and Certifications</a>
            </li>
            <li class="nav-item px-2">
                <a class="nav-link" data-bs-toggle="tab" href="#tab6">References and Past Performance</a>
            </li>
        </ul>
    </div>

    <form id="multiPageForm" name="myProfile">
        <div class="listPage mb-20">
            <!-- Tab panes -->
            <div class="tab-content pt-4">
                @* Vendor Information *@
                <div class="tab-pane active px-10" id="tab1">

                    <div class="row pb-15 pt-5">
                        <div class="col-1 rectangleIcon"></div>
                        <div class="col-5 liTabHeaders">Vendor Information</div>
                    </div>

                    <div class="row">
                        <div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="SupplierCode">Supplier Code</label>
                                <input type="text" ng-model="Vendor.SupplierCode" id="SupplierCode" name="SupplierCode" disabled>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-3 pe-8">
                            <label hidden ng-model="Vendor.SupplierIID"></label>
                            <label hidden ng-model="Vendor.LoginID"></label>
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="FirstName">Company Name<span class="required"> </span></label>
                                <input type="text" ng-model="Vendor.FirstName" id="FirstName" name="Company Name" required oninput="this.value = this.value.toUpperCase()" disabled>
                                @Html.ValidationMessageFor(m => m.FirstName)
                            </div>
                        </div><div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="VendorNickName">Company Short Name<span class="required"> </span></label>
                                <input type="text" ng-model="Vendor.VendorNickName" id="VendorNickName" name="Company Short Name" required oninput="this.value = this.value.toUpperCase()">
                                @Html.ValidationMessageFor(m => m.VendorNickName)
                            </div>
                        </div><div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="CommunicationAddress">Communication Address<span class="required"> </span></label>
                                <input type="text" ng-model="Vendor.CommunicationAddress" id="CommunicationAddress" name="Communication Address" required oninput="this.value = this.value.toUpperCase()">
                                @Html.ValidationMessageFor(m => m.CommunicationAddress)
                            </div>
                        </div>
                        <div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="PhysicalAddress">Physical Address<span class="required"> </span></label>
                                <input type="text" ng-model="Vendor.PhysicalAddress" id="PhysicalAddress" name="Physical Address" required oninput="this.value = this.value.toUpperCase()">
                                @Html.ValidationMessageFor(m => m.PhysicalAddress)
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="TelephoneNumber">Phone Number<span class="required"> </span></label>
                                <input type="text" ng-model="Vendor.TelephoneNumber" id="TelephoneNumber" name="Phone Number" required>
                                @Html.ValidationMessageFor(m => m.TelephoneNumber)
                            </div>
                        </div><div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="SupplierEmail">Email Address<span class="required"> </span></label>
                                <input type="email" ng-model="Vendor.SupplierEmail" id="SupplierEmail" name="Email Address" required>
                                @Html.ValidationMessageFor(m => m.SupplierEmail)
                            </div>
                        </div><div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="phone">Website URL<span class="required"> </span></label>
                                <input type="text" ng-model="Vendor.WebsiteURL" id="WebsiteURL" name="Website URL" required>
                                @Html.ValidationMessageFor(m => m.WebsiteURL)
                            </div>
                        </div>
                    </div>

                    @*                     <div class="row"> //Temperory Commented
                    <h4>Primary Contact Person :</h4>
                    <div class="col-2">
                    <div class="form-group row margin-top-20">
                    <label class="control-label col-md-12" for="ContactPerson">Contact Person<span class="required"> </span></label>
                    <select type="text" id="ContactPerson" ng-model="Vendor.ContactPerson" name="ContactPerson" required ng-options="item as item.Value for item in ContactPersons track by item.Key" ng-change="ContactPersonChanges(Vendor.ContactPerson.Key)">
                    <option value="">Select Contact Person</option>
                    </select>
                    </div>
                    </div>
                    <div class="col-2">
                    <div class="form-group row margin-top-20">
                    <label class="control-label col-md-12" for="ContactPersonTitle">Title</label>
                    <input type="text" id="ContactPersonTitle" name="ContactPersonTitle" pattern="[A-Z]+" disabled ng-model="ContactPersonTitle">
                    </div>
                    </div>
                    <div class="col-2">
                    <div class="form-group row margin-top-20">
                    <label class="control-label col-md-12" for="ContactPersonPhone">Phone Number</label>
                    <input type="text" id="ContactPersonPhone" name="ContactPersonPhone" disabled ng-model="ContactPersonPhone">
                    </div>
                    </div>
                    <div class="col-2">
                    <div class="form-group row margin-top-20">
                    <label class="control-label col-md-12" for="ContactPersonEmail">Email Address</label>
                    <input type="text" id="ContactPersonEmail" name="ContactPersonEmail" disabled ng-model="ContactPersonEmail">
                    </div>
                    </div>
                    </div> *@

                    <div class="button-container">
                        <button class="reset btnNext">
                            Next &ensp; <svg xmlns="http://www.w3.org/2000/svg" width="8" height="13" viewBox="0 0 8 13" fill="none">
                                <path d="M7.01148 6.34301L7.01146 6.34299L1.65514 0.989055C1.5653 0.899211 1.44393 0.849941 1.32302 0.849941C1.20509 0.849941 1.08216 0.894838 0.989901 0.990058C0.806207 1.17482 0.80654 1.47132 0.9909 1.65568L6.01038 6.67749L0.988521 11.6993C0.803826 11.884 0.803826 12.1813 0.988521 12.366C1.17321 12.5507 1.47045 12.5507 1.65514 12.366L7.01148 7.00964C7.19617 6.82494 7.19617 6.52771 7.01148 6.34301Z" fill="#316BA1" stroke="#316BA1" stroke-width="0.3" />
                            </svg>
                        </button>
                        <button class="normal" ng-click="SaveChanges(Vendor)"> Save </button>
                    </div>

                </div>

                @* Business Details *@
                <div class="tab-pane fade px-10" id="tab2">

                    <div class="row pb-15 pt-5">
                        <div class="col-1 rectangleIcon"></div>
                        <div class="col-5 liTabHeaders">Business Details</div>
                    </div>

                    <div class="row">
                        <div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="companyName">Select Business Type</label>
                                <select type="text" id="businessType" name="businessType" required ng-options="item as item.Value for item in BusinessTypes track by item.Key" ng-model="Vendor.BusinessDetail.BusinessType">
                                    <option value=""></option>
                                </select>
                            </div>
                        </div>

                        <div class="col-3 pe-8">
                            <div class="form-group row margin-top-20">
                                <label class="control-label col-md-12" for="yearEstablished">Year Established</label>
                                <input type="text" id="yearEstablished" ng-model="Vendor.BusinessDetail.YearEstablished" name="yearEstablished" maxlength="4">
                            </div>
                        </div>
                    </div>

