@{
    ViewData["Title"] = "Bid Opening";
    var settingBL = new Eduegate.Domain.Setting.SettingBL(null);
    var clientName = settingBL.GetSettingValue<string>("CLIENTINSTANCE");
    var clientFullName = settingBL.GetSettingValue<string>("CLIENT_FULL_NAME");
}

<!DOCTYPE html>
<html ng-app="Eduegate.Vendor.Portal">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <!-- Stylesheets -->
    <link rel="stylesheet" href="~/Content/font-awesome.min.css" />
    <link rel="stylesheet" href="~/Content/font-awesome.css" />
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/Site.css" />
    <link rel="stylesheet" href="~/Content/jquery-ui.css">

    <link href="~/Content/dashboard/dashboard.css" rel="stylesheet" />
    <link href="~/Content/dashboard/docs.css" rel="stylesheet" />
    <link href="~/Content/dashboard/style.bundle.css" rel="stylesheet" />
    <link href="~/Content/plugins.bundle.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/Style.css">
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">


    <!-- Scripts -->
    <script src="~/Scripts/jquery-3.4.1.js" asp-append-version="true"></script>
    <script src="~/Scripts/jquery-ui.js" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.ui.touch-punch.min.js" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.prettyPhoto.js" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.validate.js" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.custom.js" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.signalR-2.2.2.min.js" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.waypoints.min.js" asp-append-version="true"></script>
    <script src="~/Scripts/jquery.signature.js" asp-append-version="true"></script>
    <script src="~/Scripts/moment.min.js" asp-append-version="true"></script>
    <script src="~/Scripts/angular.js" asp-append-version="true"></script>
    <script src="~/Scripts/angular-route.js" asp-append-version="true"></script>
    <script src="~/Scripts/angular-animate.js" asp-append-version="true"></script>
    <script src="~/Scripts/angular-sanitize.js" asp-append-version="true"></script>
    <script src="~/Scripts/angular-moment.min.js" asp-append-version="true"></script>
    <script src="~/Scripts/angular-filter.min.js" asp-append-version="true"></script>
    <script src="~/Scripts/ng-map.min.js" asp-append-version="true"></script>
    <script src="~/app/Services/app.js" asp-append-version="true"></script>

    <script src="~/app/Directives/Datetimepicker.js?v=1.0"></script>

    <script src="~/app/Services/commonService.js" asp-append-version="true"></script>
    <script src="~/app/schedule.js" asp-append-version="true"></script>
    <script src="~/app/util.js" asp-append-version="true"></script>
    <script src="~/Scripts/bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/Scripts/respond.js" asp-append-version="true"></script>
    <script src="~/Scripts/JSLINQ.js" asp-append-version="true"></script>
    <script src="~/Scripts/Custom.js" asp-append-version="true"></script>
    <script src="~/app/utility.js" asp-append-version="true"></script>
    <script src="~/app/Controller/BidOpeningController.js" asp-append-version="true"></script>
    <script src="~/app/Controller/BidController.js" asp-append-version="true"></script>
    <script src="~/app/Controller/DataListController.js" asp-append-version="true"></script>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,700,900|Ubuntu:400,500,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,500,700,900|Ubuntu:400,500,700" rel="stylesheet">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

</head>

<body>
    <div style="text-align:center;" class="pt-1">
        @await Html.PartialAsync("_ToastMessage")
    </div>
    <div id="layout" ng-controller="BidOpeningController" ng-init="Init()">
        <div class="row pb-5 pt-3 sticky-div">
            <div class="col-3"></div>
            <div class="col"></div>
            <div class="col-3 pe-20 me-5 d-flex justify-content-end">
                <div class="profileTabHeader p-2">
                    <svg class="profilePhoto" width="41" height="41" viewBox="0 0 41 41" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <circle id="Ellipse" cx="20.5" cy="20.5" r="20.5" fill="url(#pattern0_145_139)" />
                        <defs>
                            <pattern id="pattern0_145_139" patternContentUnits="objectBoundingBox" width="1" height="1">
                                <use xlink:href="#image0_145_139" transform="translate(-0.00251256) scale(0.00502513)" />
                            </pattern>
                            <image id="image0_145_139" width="200" height="199" xlink:href="data:image/png;base64," />
                        </defs>
                    </svg>
                    <div class="profileInfo">
                        <label class="pt-4">{{BidUserDetails.UserID}}</label>
                        <p class="headerMail">{{BidUserDetails.EmailID}}</p>
                    </div>
                    <ul style="list-style-type: none;" class="pt-3">
                        <li class="dropdown">
                            <a class="nav-link" data-bs-toggle="dropdown" href="#" role="button" aria-expanded="false">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="8" viewBox="0 0 16 8" fill="none">
                                    <path d="M15.8045 0.180451C15.5439 -0.0601503 15.1225 -0.0601503 14.8619 0.180451L7.99998 6.51448L1.13812 0.180451C0.877474 -0.0601503 0.456139 -0.0601503 0.195488 0.180451C-0.0651627 0.421052 -0.0651627 0.809977 0.195488 1.05058L7.52868 7.81969C7.65868 7.93968 7.82933 8 8.00001 8C8.1707 8 8.34135 7.93968 8.47135 7.81969L15.8045 1.05058C16.0652 0.809977 16.0652 0.421052 15.8045 0.180451Z" fill="#515151" />
                                </svg>
                            </a>
                            <ul class="dropdown-menu profileDropdown">
                                <li class="m-3">
                                    <a class="dropdown-item" href="~/bid/BidLogin" style="color: #8E3131;">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="12" height="15" viewBox="0 0 12 15" fill="none">
                                            <path d="M8.97875 4.87418C9.21488 4.87418 9.40631 4.68275 9.40631 4.44662V1.28268C9.40631 0.57541 8.8309 0 8.12363 0H1.28268C0.57541 0 0 0.57541 0 1.28268V13.3114C0 14.0186 0.57541 14.594 1.28268 14.594H8.12363C8.8309 14.594 9.40631 14.0186 9.40631 13.3114V10.1474C9.40631 9.91129 9.21491 9.71986 8.97875 9.71986C8.74262 9.71986 8.55119 9.91129 8.55119 10.1474V13.3114C8.55119 13.5471 8.35939 13.7389 8.12363 13.7389H1.28268C1.04692 13.7389 0.855119 13.5471 0.855119 13.3114V1.28268C0.855119 1.04692 1.04692 0.855119 1.28268 0.855119H8.12363C8.35939 0.855119 8.55119 1.04692 8.55119 1.28268V4.44662C8.55119 4.68275 8.74262 4.87418 8.97875 4.87418Z" fill="#8E3131" />
